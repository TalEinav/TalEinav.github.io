 RowBox[{"Table", "[", 
  RowBox[{
   RowBox[{"Sum", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"Signature", "[", 
       RowBox[{"{", 
        RowBox[{"i", ",", "j", ",", "k"}], "}"}], "]"}], 
      RowBox[{"a", "[", "j", "]"}], 
      RowBox[{"b", "[", "k", "]"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"j", ",", "1", ",", "3"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"k", ",", "1", ",", "3"}], "}"}]}], "]"}], ",", 
   RowBox[{"{", 
    RowBox[{"i", ",", "1", ",", "3"}], "}"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.662061454211465*^9, 3.662061495977394*^9}, {
  3.6620615325315084`*^9, 3.6620615343293138`*^9}, {3.6620616756095266`*^9, 
  3.6620617240564704`*^9}},ExpressionUUID->"c10f02de-8813-4002-908d-\
936285ae32f5"],

Cell[CellGroupData[{

Cell[TextData[{
 "You may also remember the alternative mnemonic for the cross product ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     OverscriptBox["a", "\[RightVector]"], "\[Times]", 
     OverscriptBox["b", "\[RightVector]"]}], "=", 
    RowBox[{"Det", "[", 
     RowBox[{"(", GridBox[{
        {
         OverscriptBox["i", "^"], 
         OverscriptBox["j", "^"], 
         OverscriptBox["k", "^"]},
        {
         SubscriptBox["a", "1"], 
         SubscriptBox["a", "2"], 
         SubscriptBox["a", "3"]},
        {
         SubscriptBox["b", "1"], 
         SubscriptBox["b", "2"], 
         SubscriptBox["b", "3"]}
       }], ")"}], "]"}]}], TraditionalForm]],ExpressionUUID->
  "276e99b2-3169-4141-9e7d-a8bb58cff9e0"],
 ". Implement a version of the cross product using this form."
}], "Item",
