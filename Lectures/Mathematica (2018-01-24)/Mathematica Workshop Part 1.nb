Cell[TextData[{
 "This occurs because Plot has the attribute ",
 Cell[BoxData[
  TagBox[
   ButtonBox[
    PaneSelectorBox[{False->"HoldAll", True->
     StyleBox["HoldAll", "HyperlinkActive"]}, Dynamic[
      CurrentValue["MouseOver"]],
     BaseStyle->{"Hyperlink"},
