   RowBox[{"x", "\[Rule]", "5"}]}], "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"Equivalent", " ", "form"}], " ", "*)"}]}], "\[IndentingNewLine]", 
