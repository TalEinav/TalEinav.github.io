Cell["Kolmogorov-Smirnov Test", "Subsubsection",
 CellChangeTimes->{{3.662062448780469*^9, 
  3.662062449593671*^9}},ExpressionUUID->"7743bd9c-c2e6-4abb-865c-\
f5de5d3f9c29"],

Cell[TextData[{
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " has every distribution you will ever encounter programmed into it. Here we \
show the Kolmogorov\[Dash]Smirnov test, which computes the maximum absolute \
difference between the empirical and theoretical CDF:"
}], "Text",
 CellChangeTimes->{{3.662062603174526*^9, 3.662062621109174*^9}, {
  3.6620628111073484`*^9, 3.662062826301797*^9}, {3.689411069707918*^9, 
  3.689411070900644*^9}, {3.6894111692264447`*^9, 3.6894111728369217`*^9}, {
  3.689542069419761*^9, 
  3.6895420698729744`*^9}},ExpressionUUID->"41d0db37-e0cf-4ce7-a15d-\
3f3b52884841"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"data", "=", 
   RowBox[{"RandomVariate", "[", 
    RowBox[{
     RowBox[{"NormalDistribution", "[", "]"}], ",", "5"}], "]"}]}], 
  ";"}], "\n", 
 RowBox[{
  RowBox[{"\[ScriptCapitalH]", "=", 
   RowBox[{"KolmogorovSmirnovTest", "[", 
    RowBox[{"data", ",", 
     RowBox[{"NormalDistribution", "[", 
      RowBox[{"\[Mu]", ",", "\[Sigma]"}], "]"}], ",", 
     "\"\<HypothesisTestData\>\""}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"\[ScriptCapitalD]emp", "=", 
   RowBox[{"EmpiricalDistribution", "[", "data", "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[ScriptCapitalD]", "=", 
   RowBox[{"\[ScriptCapitalH]", "[", "\"\<FittedDistribution\>\"", "]"}]}], 
  ";"}], "\n", 
 RowBox[{
  RowBox[{"AbsD", "=", 
   RowBox[{"Abs", "[", 
    RowBox[{
     RowBox[{"CDF", "[", 
      RowBox[{"\[ScriptCapitalD]emp", ",", "data"}], "]"}], "-", 
     RowBox[{"CDF", "[", 
      RowBox[{"\[ScriptCapitalD]", ",", "data"}], "]"}]}], "]"}]}], 
  ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"MaxD", "=", 
    RowBox[{
     RowBox[{"data", "\[LeftDoubleBracket]", 
      RowBox[{"Ordering", "[", 
       RowBox[{"AbsD", ",", 
        RowBox[{"-", "1"}]}], "]"}], "\[RightDoubleBracket]"}], 
     "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}]}], ";"}], "\n", 
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{"Plot", " ", "the", " ", "CDFs"}], ",", " ", 
    RowBox[{
    "showing", " ", "the", " ", "maximum", " ", "absolute", " ", 
     "difference"}]}], " ", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{"Show", "[", 
  RowBox[{
   RowBox[{"Plot", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"CDF", "[", 
        RowBox[{"\[ScriptCapitalD]emp", ",", "x"}], "]"}], ",", 
       RowBox[{"CDF", "[", 
        RowBox[{"\[ScriptCapitalD]", ",", "x"}], "]"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"x", ",", 
       RowBox[{"-", "4"}], ",", "4"}], "}"}], ",", 
     RowBox[{"Exclusions", "\[Rule]", "None"}], ",", 
     RowBox[{"Axes", "\[Rule]", "None"}], ",", 
     RowBox[{"Frame", "\[Rule]", "True"}]}], "]"}], ",", 
   RowBox[{"Graphics", "[", 
    RowBox[{"{", 
     RowBox[{"Thick", ",", "Purple", ",", 
      RowBox[{"Line", "[", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"MaxD", ",", 
           RowBox[{"CDF", "[", 
            RowBox[{"\[ScriptCapitalD]emp", ",", "MaxD"}], "]"}]}], "}"}], 
         ",", 
         RowBox[{"{", 
          RowBox[{"MaxD", ",", 
           RowBox[{"CDF", "[", 
            RowBox[{"\[ScriptCapitalD]", ",", "MaxD"}], "]"}]}], "}"}]}], 
        "}"}], "]"}]}], "}"}], "]"}]}], "]"}]}], "Input",
 CellChangeTimes->{3.6620628488072653`*^9},
 CellID->25563453,ExpressionUUID->"142c8ae8-8476-4a6b-95b2-28dfd96eeaa1"]
}, Closed]],

