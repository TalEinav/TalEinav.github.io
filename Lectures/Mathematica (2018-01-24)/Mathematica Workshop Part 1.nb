   3.6624322651290817`*^9}},ExpressionUUID->"7545733d-45ab-449d-92d4-\
af494d35be75"],

Cell[BoxData[
 RowBox[{"list", "=", 
  RowBox[{"{", 
   RowBox[{"1", ",", "2", ",", "3", ",", "4", ",", "5"}], "}"}]}]], "Input",
 CellChangeTimes->{{3.6229248057990313`*^9, 
  3.6229248296242733`*^9}},ExpressionUUID->"7de6f15a-a228-4612-924c-\
f1dbd4f6e7f2"],

