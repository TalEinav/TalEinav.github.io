  3.622935188080792*^9}},ExpressionUUID->"c25a3dc1-4014-4528-b1e8-\
6e85d3f70734"],

Cell[BoxData[
 RowBox[{"poly", "=", 
  RowBox[{"4", "+", 
   RowBox[{"5", " ", 
    SuperscriptBox["x", "2"]}], "-", 
   RowBox[{"7", " ", 
    SuperscriptBox["x", "3"]}], "+", 
