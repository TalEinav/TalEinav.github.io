       RowBox[{"{", 
        RowBox[{"i", ",", "j", ",", "k"}], "}"}], "]"}], 
      RowBox[{"a", "[", "j", "]"}], 
      RowBox[{"b", "[", "k", "]"}]}], ",", 
     RowBox[{"{", 
