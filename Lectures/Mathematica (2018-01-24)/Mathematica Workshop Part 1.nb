       RowBox[{"freq", " ", "x"}], "]"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"x", ",", "0", ",", "6"}], "}"}], ",", 
     RowBox[{"PlotStyle", "\[Rule]", 
      RowBox[{"{", 
       RowBox[{"color", ",", 
        RowBox[{"Dashing", "[", "dashing", "]"}], ",", 
        RowBox[{"Thickness", "[", "thickness", "]"}]}], "}"}]}], ",", 
     RowBox[{"Axes", "\[Rule]", "axes"}], ",", 
