       RowBox[{"3", ",", "2"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"5", ",", "4"}], "}"}]}], "}"}]}], ";"}], "\n", 
  RowBox[{"(*", " ", 
   RowBox[{
   "Fit", " ", "data", " ", "to", " ", "a", " ", "linear", " ", "curve"}], 
   " ", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"line", " ", "=", " ", 
    RowBox[{"Fit", "[", 
     RowBox[{"data", ",", " ", 
      RowBox[{"{", 
       RowBox[{"1", ",", "x"}], "}"}], ",", "x"}], "]"}]}], ";"}], 
  "\[IndentingNewLine]", 
