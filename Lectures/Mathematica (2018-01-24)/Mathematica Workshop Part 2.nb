    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"mp", "=", 
      RowBox[{
       RowBox[{"Plus", "@@", "l"}], "/", "7"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"qs", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"\[ScriptCapitalF]", "[", 
          RowBox[{"1", ",", "#", ",", "\[ScriptF]"}], "]"}], "&"}], ")"}], "/@",
        "l"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"mpF", "=", 
      RowBox[{"\[ScriptCapitalF]", "[", 
