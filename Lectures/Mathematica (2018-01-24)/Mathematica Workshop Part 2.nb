Cell[TextData[{
 "Plot the solutions to the damped harmonic oscillator (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"x", "''"}], "[", "t", "]"}], "+", 
     RowBox[{"\[Gamma]", " ", 
      RowBox[{
       RowBox[{"x", "'"}], "[", "t", "]"}]}], "+", 
     RowBox[{
      SuperscriptBox["\[Omega]", "2"], 
      RowBox[{"x", "[", "t", "]"}]}]}], "=", "0"}], TraditionalForm]],
  ExpressionUUID->"4eee646e-d6ad-4bfb-a6fe-ce760a0cd47c"],
 ") with the initial condition ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"x", "'"}], "[", "0", "]"}], "=", "0"}], TraditionalForm]],
  ExpressionUUID->"f0fd9a7c-7471-4a23-826d-2c7325815c92"],
 " and ",
