Cell[TextData[{
 "There are many more ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " commands that I did not cover (for example, using underscores and \
periods). Rather than covering each one individually, the following program \
takes a list of string characters and spits out all syntactically correct ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " expressions"
}], "Text",
 CellChangeTimes->{{3.662064035512271*^9, 
  3.66206406453596*^9}},ExpressionUUID->"8e96573a-d0d5-46bf-b224-\
a908d84e9bfc"],

Cell[BoxData[
 RowBox[{
  RowBox[{"AllSyntacticallyCorrectExpressions", "[", 
   RowBox[{"symbolsUsed_", "?", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"VectorQ", "[", 
       RowBox[{"#", ",", "StringQ"}], "]"}], "&"}], ")"}]}], "]"}], ":=", 
  RowBox[{"TableForm", "[", 
   StyleBox[
    RowBox[{"(*", " ", 
     RowBox[{"format", " ", "output", " ", "nicely"}], " ", "*)"}], 
    "CodeComment"], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"StringDrop", "[", 
        RowBox[{
         RowBox[{"StringDrop", "[", 
          RowBox[{
           RowBox[{"ToString", "[", 
            RowBox[{"InputForm", "[", "#", "]"}], "]"}], ",", "5"}], "]"}], 
         ",", 
         RowBox[{"-", "1"}]}], "]"}], ",", 
       RowBox[{"StringDrop", "[", 
        RowBox[{
         RowBox[{"StringDrop", "[", 
          RowBox[{
           RowBox[{"ToString", "[", 
            RowBox[{"FullForm", "[", "#", "]"}], "]"}], ",", "5"}], "]"}], 
         ",", 
         RowBox[{"-", "1"}]}], "]"}]}], "}"}], "&"}], "/@", 
    RowBox[{"Union", "[", 
     RowBox[{"Flatten", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Union", "[", 
         StyleBox[
          RowBox[{"(*", " ", 
           RowBox[{"test", " ", "syntax"}], " ", "*)"}], "CodeComment"], " ", 
         "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"SyntaxQ", "[", "#", "]"}], ",", 
             RowBox[{"ToHeldExpression", "[", "#", "]"}], ",", 
             RowBox[{"{", "}"}]}], "]"}], "&"}], "/@", 
          RowBox[{"StringJoin", "/@", 
           StyleBox[
            RowBox[{"(*", " ", 
             RowBox[{"all", " ", "combinations"}], " ", "*)"}], "CodeComment"], 
           RowBox[{"Permutations", "[", 
            RowBox[{"Join", "[", 
             RowBox[{"#", ",", 
              RowBox[{"Table", "[", 
               RowBox[{"\"\< \>\"", ",", 
                RowBox[{"{", 
                 RowBox[{
                  RowBox[{"Length", "[", "#", "]"}], "-", "1"}], "}"}]}], 
               "]"}]}], "]"}], "]"}]}]}], "]"}], "&"}], "/@", 
       RowBox[{"DeleteCases", "[", 
        RowBox[{
         RowBox[{"Sort", "[", 
          RowBox[{"Distribute", "[", 
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{"{", 
               RowBox[{
                RowBox[{"{", "}"}], ",", 
                RowBox[{"{", "#", "}"}]}], "}"}], "&"}], "/@", 
             "symbolsUsed"}], ",", "List", ",", "List", ",", "List", ",", 
            "Join"}], "]"}], "]"}], ",", 
         RowBox[{"{", "}"}]}], "]"}]}], "]"}], "]"}]}], "]"}]}]], "Input",
 GeneratedCell->False,
 CellChangeTimes->{{3.657221464481073*^9, 3.657221464481073*^9}, {
