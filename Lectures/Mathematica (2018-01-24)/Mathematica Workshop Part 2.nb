    "\[IndentingNewLine]", 
    RowBox[{"lm", "=", 
     RowBox[{"LinearModelFit", "[", 
      RowBox[{"reg", ",", 
       RowBox[{"{", 
        RowBox[{"1", ",", "x", ",", "y"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"x", ",", "y"}], "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"bf", "=", 
     RowBox[{"lm", "[", "\"\<BestFitParameters\>\"", "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"<|", 
       RowBox[{
        RowBox[{"\"\<center\>\"", "\[Rule]", 
