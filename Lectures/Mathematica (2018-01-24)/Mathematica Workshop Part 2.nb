Cell[CellGroupData[{

Cell["Step 2: Dividing the Faces into Triangles", "Subsubsection",ExpressionUUID->"95952374-3c71-4ed5-97a9-93d85f0648f8"],

Cell["\<\
 This divides the faces of the dodecahedron into isosceles triangles. \
\>", "Text",ExpressionUUID->"a113914c-a1c6-44d7-8b28-0020f0693aa7"],

Cell[BoxData[
 RowBox[{
  RowBox[{"dodecahedronTriangles", "=", 
   RowBox[{"N", "[", 
    RowBox[{"Flatten", "[", 
     RowBox[{
      RowBox[{"Function", "[", 
       RowBox[{"l", ",", 
        RowBox[{"Module", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"mp", "=", 
            RowBox[{"Mean", "@", 
             RowBox[{
             "l", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}]}]}], 
           "}"}], ",", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"Polygon", "[", 
              RowBox[{"Append", "[", 
               RowBox[{"#", ",", "mp"}], "]"}], "]"}], "&"}], ")"}], "/@", 
           "\[IndentingNewLine]", 
           RowBox[{"Partition", "[", 
            RowBox[{
             RowBox[{"Append", "[", 
              RowBox[{
               RowBox[{
               "l", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], 
               ",", 
               RowBox[{"l", "\[LeftDoubleBracket]", 
                RowBox[{"1", ",", "1"}], "\[RightDoubleBracket]"}]}], "]"}], 
             ",", "2", ",", "1"}], "]"}]}]}], "]"}]}], "]"}], "/@", 
      RowBox[{"N", "[", 
       RowBox[{
        RowBox[{"Map", "[", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"PolyhedronData", "[", 
             RowBox[{
             "\"\<Dodecahedron\>\"", ",", "\"\<VertexCoordinates\>\""}], 
             "]"}], "\[LeftDoubleBracket]", "#", "\[RightDoubleBracket]"}], 
           "&"}], ",", "\[IndentingNewLine]", 
          RowBox[{"Polygon", "/@", 
           RowBox[{"PolyhedronData", "[", 
            RowBox[{"\"\<Dodecahedron\>\"", ",", "\"\<FaceIndices\>\""}], 
            "]"}]}], ",", 
          RowBox[{"{", 
           RowBox[{"-", "1"}], "}"}]}], "]"}], ",", "100"}], "]"}]}], "]"}], 
    "]"}]}], ";"}]], "Input",
 CellChangeTimes->{
  3.3858453097237306`*^9, {3.603234870960532*^9, 
   3.603234893775532*^9}},ExpressionUUID->"3a15f680-e8f6-42df-aeaf-\
1123e1569073"],

