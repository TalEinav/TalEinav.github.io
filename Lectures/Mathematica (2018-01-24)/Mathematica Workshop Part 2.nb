Cell[BoxData[{
 RowBox[{"Clear", "[", 
  RowBox[{"f", ",", "x", ",", "y", ",", "t", ",", "z"}], 
  "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"path", "[", "t_", "]"}], ":=", 
  RowBox[{"2", "+", 
   RowBox[{"Exp", "[", 
    RowBox[{"\[ImaginaryI]", " ", "t"}], "]"}]}]}], "\n", 
 RowBox[{
  RowBox[{"f", "[", "z_", "]"}], ":=", 
  FractionBox[
   RowBox[{"Sin", "[", "z", "]"}], 
   RowBox[{
    RowBox[{"(", 
     RowBox[{"z", "-", "2"}], ")"}], "^", "3"}]]}], "\n", 
 RowBox[{"Show", "[", 
  RowBox[{
   RowBox[{"Plot3D", "[", 
    RowBox[{
     RowBox[{"Norm", "@", 
      RowBox[{"f", "[", 
       RowBox[{"x", "+", 
        RowBox[{"I", "*", "y"}]}], "]"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"x", ",", "0", ",", "4"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"y", ",", 
       RowBox[{"-", "2"}], ",", "2"}], "}"}], ",", 
     RowBox[{"ColorFunction", "\[Rule]", 
      RowBox[{"Function", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"x", ",", "y", ",", "z"}], "}"}], ",", 
        RowBox[{"Hue", "@", 
         RowBox[{"Norm", "[", 
          RowBox[{"x", "+", 
           RowBox[{"I", " ", "y"}]}], "]"}]}]}], "]"}]}], ",", 
     RowBox[{"AxesLabel", "\[Rule]", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"Re", "[", "z", "]"}], ",", 
        RowBox[{"Im", "[", "z", "]"}], ",", 
        RowBox[{"Abs", "[", "z", "]"}]}], "}"}]}], ",", 
     RowBox[{"LabelStyle", "\[Rule]", "14"}], ",", 
     RowBox[{"PlotLabel", "\[Rule]", 
      RowBox[{"Style", "[", 
       RowBox[{
        RowBox[{"HoldForm", "[", 
         RowBox[{
          RowBox[{"Sin", "[", "z", "]"}], "/", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"z", "-", "2"}], ")"}], "^", "3"}]}], "]"}], ",", "14"}], 
       "]"}]}], ",", 
     RowBox[{"PlotRange", "\[Rule]", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"0.5", ",", "3.5"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"-", "1.5"}], ",", "1.5"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"0.", ",", "5"}], "}"}]}], "}"}]}]}], "]"}], ",", 
   RowBox[{"ParametricPlot3D", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"2", "+", 
        RowBox[{"Cos", "[", "\[Theta]", "]"}]}], ",", 
       RowBox[{"Sin", "[", "\[Theta]", "]"}], ",", 
       RowBox[{"Norm", "@", 
        RowBox[{"f", "[", 
         RowBox[{"2", "+", 
          RowBox[{"Cos", "[", "\[Theta]", "]"}], "+", 
          RowBox[{"I", "*", 
           RowBox[{"Sin", "[", "\[Theta]", "]"}]}]}], "]"}]}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"\[Theta]", ",", "0", ",", 
       RowBox[{"2", "\[Pi]"}]}], "}"}], ",", 
     RowBox[{"PlotStyle", "\[Rule]", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"Thickness", "[", "0.015", "]"}], ",", "Yellow"}], "}"}]}]}], 
    "]"}]}], "]"}]}], "Input",
