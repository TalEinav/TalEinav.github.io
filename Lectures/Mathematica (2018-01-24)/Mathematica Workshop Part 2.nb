Cell[TextData[{
 StyleBox["Example: Discretization of the steady-state 2D heat equation leads \
to linear equations: \n",
  FontWeight->"Plain"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox[
      RowBox[{
       SuperscriptBox["\[PartialD]", "2"], "u"}], 
      RowBox[{"\[PartialD]", 
       SuperscriptBox["x", "2"]}]], "+", 
     FractionBox[
      RowBox[{
       SuperscriptBox["\[PartialD]", "2"], "u"}], 
      RowBox[{"\[PartialD]", 
       SuperscriptBox["y", "2"]}]]}], "=", 
    RowBox[{
     RowBox[{"q", "\[Implies]", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["u", 
          RowBox[{
           RowBox[{"i", "-", "1"}], ",", "j"}]], "-", 
         RowBox[{"2", 
          SubscriptBox["u", 
           RowBox[{"i", ",", "j"}]]}], "+", 
         SubscriptBox["u", 
          RowBox[{
           RowBox[{"i", "+", "1"}], ",", "j"}]]}], ")"}], "+", 
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["u", 
          RowBox[{"i", ",", 
           RowBox[{"j", "-", "1"}]}]], "-", 
         RowBox[{"2", 
          SubscriptBox["u", 
           RowBox[{"i", ",", "j"}]]}], "+", 
         SubscriptBox["u", 
          RowBox[{"i", ",", 
           RowBox[{"j", "+", "1"}]}]]}], ")"}]}]}], "=", "q"}]}], 
   TraditionalForm]],ExpressionUUID->"6c7d889f-331e-4960-b6ec-37a5837e7d05"]
}], "Text",
