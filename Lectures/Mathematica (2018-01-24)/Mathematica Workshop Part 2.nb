 RowBox[{"Total", "[", 
  RowBox[{"{", 
   RowBox[{"2", ",", "3"}], "}"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{"Total", "[", 
  RowBox[{"{", 
   RowBox[{"2", ",", "3", ",", "5"}], "}"}], "]"}]}], "Input",
 CellChangeTimes->{{3.6229368262110834`*^9, 
  3.622936837601104*^9}},ExpressionUUID->"e59f9c3d-fd3e-4550-a12f-\
993aef0bbf16"],

Cell[TextData[{
 "We can have a function act upon each individual element in a list by using ",
 Cell[BoxData[
  TagBox[
   ButtonBox[
    PaneSelectorBox[{False->"Map", True->
     StyleBox["Map", "HyperlinkActive"]}, Dynamic[
      CurrentValue["MouseOver"]],
     BaseStyle->{"Hyperlink"},
     FrameMargins->0,
     ImageSize->Automatic],
    BaseStyle->"Link",
    ButtonData->"paclet:ref/Map",
    ButtonNote->"paclet:ref/Map"],
   Annotation[#, "paclet:ref/Map", "Hyperlink"]& ]], "Output",
  CellChangeTimes->{3.6229367921834235`*^9, 3.622936907556187*^9},
  ExpressionUUID->"472f0b8a-07a9-4368-8781-4a122338b0dc"],
 " which has the handy shorthand (/@)"
}], "Item",
 CellChangeTimes->{{3.6229368644507513`*^9, 3.6229368732823668`*^9}, {
  3.62293690375597*^9, 
  3.622936944443348*^9}},ExpressionUUID->"9e57ff50-c445-4597-bee2-\
5fc7bd148a9b"],

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"Using", " ", "Map"}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Map", "[", 
    RowBox[{"Total", ",", "list"}], "]"}], "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"Equivalently", ",", " ", 
