Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ScriptCapitalF]", "[", 
     RowBox[{"\[Alpha]_", ",", " ", "xyz_", ",", "f_"}], "]"}], ":=", 
    RowBox[{"\[Alpha]", " ", "xyz", " ", 
     RowBox[{"f", "[", 
      FractionBox[
       RowBox[{
        SqrtBox[
         RowBox[{"xyz", ".", "xyz"}]], "-", 
        SubscriptBox["\[Rho]", "min"]}], 
       RowBox[{
        SubscriptBox["\[Rho]", "max"], "-", 
        SubscriptBox["\[Rho]", "min"]}]], "]"}]}]}], TraditionalForm]],
  ExpressionUUID->"9faf2af8-79bc-45f7-8658-3cab189f6fe9"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"addHatToL", "[", 
     RowBox[{"L", ":", 
      RowBox[{"Polygon", "[", "l_", "]"}]}], "]"}], ":=", 
    "\[IndentingNewLine]", 
    RowBox[{"Module", "[", 
     RowBox[{"...", "\[InvisibleComma]", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"qs", "=", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"\[ScriptCapitalF]", "[", 
            RowBox[{"1", ",", "\[NumberSign]", ",", "\[ScriptF]"}], "]"}], 
           "&"}], ")"}], "/@", "l"}]}], ";"}], "\[InvisibleComma]", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"mpF", "=", 
        RowBox[{"\[ScriptCapitalF]", "[", 
         RowBox[{"1.06", ",", "mp", ",", "\[ScriptF]"}], "]"}]}], ";"}], 
      "\[InvisibleComma]", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"rs", "=", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"\[ScriptCapitalF]", "[", 
            RowBox[{"1.06", ",", 
             RowBox[{"mp", "+", 
              RowBox[{"0.6", " ", 
               RowBox[{"(", 
                RowBox[{"\[NumberSign]", "-", "mp"}], ")"}]}]}], ",", 
             "\[ScriptF]"}], "]"}], "&"}], ")"}], "/@", "l"}]}], ";"}], 
      "\[InvisibleComma]", "\[IndentingNewLine]", "..."}], 
     "\[IndentingNewLine]", "]"}]}], TraditionalForm]],ExpressionUUID->
  "c1530176-49b7-4d01-a536-238d3bced800"]
}], "Text",
