Cell[TextData[{
 "The function ",
 StyleBox["mapPointToDodecahedronTriangle", "InlineInput"],
 " maps points ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{"x", ",", "y"}], ")"}], TraditionalForm]],ExpressionUUID->
  "2ce6603e-6c46-49fc-bad7-29dd71f396b7"],
 " with ",
 Cell[BoxData[
  FormBox[
   RowBox[{"0", "\[LessEqual]", "x", "\[LessEqual]", "1"}], TraditionalForm]],
  ExpressionUUID->"270b3e8d-e17a-4f7b-a2bd-0654e00742f4"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"0", "\[LessEqual]", "y", "\[LessEqual]", 
