Cell[TextData[{
 "Prove that the following result is correct. (The operation ",
 Cell[BoxData["\[CirclePlus]"], "Input",
  CellChangeTimes->{{3.661834114976201*^9, 3.6618341150074015`*^9}},
  ExpressionUUID->"8fad5331-3456-4240-804a-f31ba25e724a"],
 " has no built-in meaning in ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 ", just treat it as a generic operation whose definition is given by the ",
 Cell[BoxData["ForAll"], "Input",
  CellChangeTimes->{{3.661834140617047*^9, 3.661834141241048*^9}},
  ExpressionUUID->"5dba9444-cf11-4612-abdc-472752cd37ed"],
 " statement.)"
}], "Text",
 CellChangeTimes->{{3.661834022819632*^9, 3.661834037976259*^9}, {
  3.6618340985161724`*^9, 3.6618341441582527`*^9}, {3.662089813864104*^9, 
  3.6620898328481894`*^9}},ExpressionUUID->"51f4c694-9d62-4384-966f-\
a0410d1bf1d0"],

Cell[BoxData[
 RowBox[{"FullSimplify", "[", 
  RowBox[{
   RowBox[{
    RowBox[{"x", "\[CirclePlus]", "x"}], "\[Equal]", 
    RowBox[{"x", "\[CirclePlus]", "y"}]}], ",", 
   RowBox[{"ForAll", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"a", ",", "b"}], "}"}], ",", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{"a", "\[CirclePlus]", "a"}], ")"}], "\[CirclePlus]", "b"}], "==",
       "a"}]}], "]"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.661834084339347*^9, 
  3.6618341105281935`*^9}},ExpressionUUID->"b7ac50e5-11e7-459d-b1ca-\
c670a7461248"],

Cell[CellGroupData[{

Cell["\<\
Surprising, isn\[CloseCurlyQuote]t it? It turns out that the second relation \
really implies the first, shockingly enough (no need to assume associativity, \
transitivity, or anything of the sort). To see the proof, check out the cell \
hidden below this one!\
\>", "Text",
 CellGroupingRules->{GroupTogetherGrouping, 10000.},
 CellChangeTimes->{{3.6061741477502637`*^9, 3.6061742078562737`*^9}, 
   3.6061742839708843`*^9},ExpressionUUID->"a307c1ed-419b-4a00-b8ed-\
f221cc54e1d7"],

Cell[TextData[{
 "So...couldn\[CloseCurlyQuote]t figure it out yourself, could you? Well, \
here is the proof. \n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"a", "\[CirclePlus]", "a"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"a", "\[CirclePlus]", "a"}], ")"}], "\[CirclePlus]", 
        RowBox[{"(", 
         RowBox[{"a", "\[CirclePlus]", "a"}], ")"}]}], ")"}], "\[CirclePlus]",
       "b"}], "=", 
     RowBox[{
      RowBox[{"(", "a", ")"}], "\[CirclePlus]", "b"}]}]}], TraditionalForm]],
  ExpressionUUID->"06db5e67-88d6-4295-bc26-65589a2083fb"]
}], "Text",
 CellGroupingRules->{GroupTogetherGrouping, 10000.},
 CellChangeTimes->{{3.6061742099364815`*^9, 3.6061742839708843`*^9}, 
   3.662434418754461*^9, 
   3.7255542747761936`*^9},ExpressionUUID->"a7375b75-f463-427d-b2de-\
b38a1b8f2aec"]
}, Closed]]
}, Closed]]
}, Closed]],

