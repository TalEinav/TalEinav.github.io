Cell[TextData[{
 StyleBox["Solution",
  FontVariations->{"Underline"->True}],
 "\nConservation of momentum shows that ",
 Cell[BoxData[
  FormBox["B", TraditionalForm]]],
 " and ",
 Cell[BoxData[
  FormBox["C", TraditionalForm]]],
 " have equal and opposite momentum, ",
 Cell[BoxData[
  FormBox["p", TraditionalForm]]],
 ". Thus"
}], "Text",
 CellChangeTimes->{{3.6311441613080025`*^9, 3.631144190606678*^9}}],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    FractionBox[
     SubsuperscriptBox["E", "B", "2"], 
     SuperscriptBox["c", "2"]], "-", 
    RowBox[{
     SubsuperscriptBox["M", "B", "2"], 
     SuperscriptBox["c", "2"]}]}], "=", 
   RowBox[{
    SuperscriptBox["p", "2"], "=", 
    RowBox[{
     FractionBox[
      SubsuperscriptBox["E", "C", "2"], 
      SuperscriptBox["c", "2"]], "-", 
     RowBox[{
      SubsuperscriptBox["M", "C", "2"], 
      SuperscriptBox["c", "2"]}]}]}]}], TraditionalForm]]]], "TextNumbered",
 CellChangeTimes->{{3.6311441914247246`*^9, 3.6311442161801405`*^9}},
 CellTags->"eq1"],

Cell["Conservation of energy yields", "Text",
 CellChangeTimes->{{3.63114422001236*^9, 3.6311442233045483`*^9}}],

