 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["M", "A"], "c"}], "=", 
   RowBox[{
    FractionBox[
     SubscriptBox["E", "B"], "c"], "+", 
    FractionBox[
     SubscriptBox["E", "C"], "c"]}]}], TraditionalForm]]]], "TextNumbered",
 CellChangeTimes->{{3.6311441914247246`*^9, 3.6311442472579184`*^9}}],

Cell[TextData[{
 "Solving these two equations for ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["E", "B"], TraditionalForm]]],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["E", "C"], TraditionalForm]]],
 " yields"
}], "Text",
 CellChangeTimes->{{3.6311442681531134`*^9, 3.631144277071624*^9}}],

