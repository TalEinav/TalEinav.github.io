Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Solve", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{
       FractionBox[
        SuperscriptBox["EB", "2"], 
        SuperscriptBox["c", "2"]], "-", 
