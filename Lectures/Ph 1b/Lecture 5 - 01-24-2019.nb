   SubscriptBox["E", "C"], "=", 
   FractionBox[
    RowBox[{
     SuperscriptBox["c", "2"], " ", 
     RowBox[{"(", 
      RowBox[{
       SubsuperscriptBox["M", "A", "2"], "-", 
       SubsuperscriptBox["M", "B", "2"], "+", 
       SubsuperscriptBox["M", "C", "2"]}], ")"}]}], 
    RowBox[{"2", " ", 
     SubscriptBox["M", "A"]}]]}], TraditionalForm]]]], "TextNumbered",
