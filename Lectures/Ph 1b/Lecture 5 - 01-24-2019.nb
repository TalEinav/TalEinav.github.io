Cell[TextData[{
 "We could also have solved this problem by explicitly writing out the \
velocities of ",
 Cell[BoxData[
  FormBox["B", TraditionalForm]]],
 " and ",
 Cell[BoxData[
  FormBox["C", TraditionalForm]]],
 " and then solving for their momentum, as shown using ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " below"
}], "Text",
 CellChangeTimes->{{3.6311441266020174`*^9, 3.631144132712367*^9}, {
   3.6311445753926864`*^9, 3.6311445989940367`*^9}, 3.631146382349038*^9}],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"$Assumptions", "=", 
   RowBox[{
    RowBox[{"0", "<", "MC", "<", "MB", "<", "MA"}], "&&", 
    RowBox[{"0", "<", "c"}]}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"res", "=", 
    RowBox[{"First", "@", 
     RowBox[{"FullSimplify", "@", 
      RowBox[{"Solve", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{
            RowBox[{"MA", " ", "c"}], "\[Equal]", 
            RowBox[{
             RowBox[{"\[Gamma]B", " ", "MB", " ", "c"}], "+", 
             RowBox[{"\[Gamma]C", " ", "MC", " ", "c"}]}]}], ",", 
           RowBox[{
            RowBox[{"\[Gamma]B", " ", "MB", " ", "vB"}], "\[Equal]", 
            RowBox[{"\[Gamma]C", " ", "MC", " ", "vC"}]}]}], "}"}], "/.", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"\[Gamma]B", "\[Rule]", 
            FractionBox["1", 
             SuperscriptBox[
              RowBox[{"(", 
               RowBox[{"1", "-", 
                FractionBox[
                 SuperscriptBox["vB", "2"], 
                 SuperscriptBox["c", "2"]]}], ")"}], 
              RowBox[{"1", "/", "2"}]]]}], ",", 
           RowBox[{"\[Gamma]C", "\[Rule]", 
            FractionBox["1", 
             SuperscriptBox[
              RowBox[{"(", 
               RowBox[{"1", "-", 
                FractionBox[
                 SuperscriptBox["vC", "2"], 
                 SuperscriptBox["c", "2"]]}], ")"}], 
              RowBox[{"1", "/", "2"}]]]}]}], "}"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"vB", ",", "vC"}], "}"}]}], "]"}]}]}]}], ";"}], "\n", 
  RowBox[{"(*", " ", 
   RowBox[{"Momentum", " ", "of", " ", 
    SubscriptBox["M", "B"]}], " ", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{"FullSimplify", "[", 
  RowBox[{
   RowBox[{
    RowBox[{"\[Gamma]B", " ", "MB", " ", "vB"}], "/.", 
    RowBox[{"\[Gamma]B", "\[Rule]", 
     FractionBox["1", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"1", "-", 
         FractionBox[
          SuperscriptBox["vB", "2"], 
          SuperscriptBox["c", "2"]]}], ")"}], 
       RowBox[{"1", "/", "2"}]]]}]}], "/.", "res"}], "]"}]}], "Input",
 CellChangeTimes->{{3.631142952747876*^9, 3.63114308306133*^9}, {
   3.6311432085605083`*^9, 3.6311432991866913`*^9}, {3.6311437332625194`*^9, 
   3.6311438025674834`*^9}, {3.6311438804349375`*^9, 
   3.6311439078185034`*^9}, {3.631143955339222*^9, 3.6311439555392327`*^9}, 
   3.6311439913562813`*^9, {3.6311440476705027`*^9, 3.6311440593371696`*^9}, {
   3.6311444998953686`*^9, 3.6311445012954483`*^9}},
 FontSize->8],

Cell[BoxData[
 FractionBox[
  RowBox[{"c", " ", 
   SqrtBox[
    RowBox[{
     RowBox[{"(", 
      RowBox[{"MA", "-", "MB", "-", "MC"}], ")"}], " ", 
     RowBox[{"(", 
      RowBox[{"MA", "+", "MB", "-", "MC"}], ")"}], " ", 
     RowBox[{"(", 
      RowBox[{"MA", "-", "MB", "+", "MC"}], ")"}], " ", 
     RowBox[{"(", 
      RowBox[{"MA", "+", "MB", "+", "MC"}], ")"}]}]]}], 
  RowBox[{"2", " ", "MA"}]]], "Output",
