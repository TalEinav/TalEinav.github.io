Cell[TextData[{
 StyleBox["Solution",
  FontVariations->{"Underline"->True}],
 "\nFor this surface, ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "=", "2"}], TraditionalForm]]],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[DifferentialD]", 
     OverscriptBox["a", "\[RightVector]"]}], "=", 
    RowBox[{
     RowBox[{"\[DifferentialD]", "y"}], 
     RowBox[{"\[DifferentialD]", "z"}], 
     OverscriptBox["x", "^"]}]}], TraditionalForm]]],
 " so that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     OverscriptBox["v", "\[RightVector]"], "\[CenterDot]", 
     RowBox[{"\[DifferentialD]", 
      OverscriptBox["a", "\[RightVector]"]}]}], "=", 
    RowBox[{"4", "z", 
     RowBox[{"\[DifferentialD]", "y"}], 
     RowBox[{"\[DifferentialD]", "z"}]}]}], TraditionalForm]]],
 ". Therefore, the surface integral equals"
}], "Text",
 CellChangeTimes->{{3.629614882385314*^9, 3.629614955201393*^9}}],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["\[Integral]", "S"], 
    RowBox[{
     OverscriptBox["v", "\[RightVector]"], "\[CenterDot]", 
     RowBox[{"\[DifferentialD]", 
      OverscriptBox["a", "\[RightVector]"]}]}]}], "=", 
   RowBox[{
    RowBox[{
     SubsuperscriptBox["\[Integral]", "0", "2"], 
     RowBox[{
      SubsuperscriptBox["\[Integral]", "0", "2"], 
      RowBox[{"4", "z", 
       RowBox[{"\[DifferentialD]", "y"}], 
       RowBox[{"\[DifferentialD]", "z"}]}]}]}], "=", "16"}]}], 
  TraditionalForm]]]], "TextNumbered",
 CellChangeTimes->{{3.6296142236183734`*^9, 3.6296142305399923`*^9}, {
  3.6296149637728167`*^9, 3.629614993181158*^9}}],

Cell[TextData[{
 "Had we chosen the ",
 Cell[BoxData[
  FormBox[
   RowBox[{"-", 
    OverscriptBox["x", "^"]}], TraditionalForm]]],
 " to denote the positive direction for the integral, we would have found the \
negative of this answer. \[EmptySquare] "
}], "Text",
 CellChangeTimes->{{3.6296150390545583`*^9, 3.6296150404569616`*^9}, {
  3.6296150928099384`*^9, 3.6296151131786385`*^9}}]
}, Open  ]],

Cell[CellGroupData[{

Cell["Volume Integral", "Subsection",
 CellChangeTimes->{{3.629611149286641*^9, 3.629611153437248*^9}, {
  3.6296129210431185`*^9, 3.6296129223067207`*^9}}],

Cell["A volume integral is an expression of the form ", "Text",
 CellChangeTimes->{3.629615148576782*^9}],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox["\[Integral]", "\[ScriptCapitalV]"], 
   RowBox[{"T", 
    RowBox[{"\[DifferentialD]", "\[Tau]"}]}]}], 
  TraditionalForm]]]], "TextNumbered",
 CellChangeTimes->{{3.629615150791986*^9, 3.6296151566419964`*^9}}],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]]],
 " is a scalar function and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[DifferentialD]", "\[Tau]"}], TraditionalForm]]],
 " is an infinitesimal volume element. In Cartesian coordinates, "
}], "Text",
 CellChangeTimes->{{3.629615161446805*^9, 3.629615165988413*^9}, {
  3.6926528271360464`*^9, 3.6926528359985533`*^9}}],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"\[DifferentialD]", "\[Tau]"}], "=", 
   RowBox[{
    RowBox[{"\[DifferentialD]", "x"}], 
    RowBox[{"\[DifferentialD]", "y"}], 
    RowBox[{"\[DifferentialD]", "z"}]}]}], TraditionalForm]]]], "TextNumbered",
 CellChangeTimes->{{3.6296151679384165`*^9, 3.629615176473631*^9}}],

Cell[TextData[{
 "For example, if ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]]],
 " is the density of a substance (which might vary from point to point), then \
the volume integral would give the total mass. You can also take the volume \
integral of a vector function"
}], "Text",
 CellChangeTimes->{{3.6296151937448616`*^9, 3.629615214701699*^9}}],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["\[Integral]", "\[ScriptCapitalV]"], 
    RowBox[{
     OverscriptBox["A", "\[RightVector]"], 
     RowBox[{"\[DifferentialD]", "\[Tau]"}]}]}], "=", 
   RowBox[{
    RowBox[{
     OverscriptBox["x", "^"], 
     RowBox[{
      SubscriptBox["\[Integral]", "\[ScriptCapitalV]"], 
      RowBox[{
       SubscriptBox["A", "x"], 
       RowBox[{"\[DifferentialD]", "\[Tau]"}]}]}]}], "+", 
    RowBox[{
     OverscriptBox["y", "^"], 
     RowBox[{
      SubscriptBox["\[Integral]", "\[ScriptCapitalV]"], 
      RowBox[{
       SubscriptBox["A", "y"], 
       RowBox[{"\[DifferentialD]", "\[Tau]"}]}]}]}], "+", 
    RowBox[{
     OverscriptBox["z", "^"], 
     RowBox[{
      SubscriptBox["\[Integral]", "\[ScriptCapitalV]"], 
      RowBox[{
       SubscriptBox["A", "z"], 
       RowBox[{"\[DifferentialD]", "\[Tau]"}]}]}]}]}]}], 
  TraditionalForm]]]], "TextNumbered",
 CellChangeTimes->{{3.629615150791986*^9, 3.6296151566419964`*^9}, {
  3.629615223158914*^9, 3.6296152708747983`*^9}}],

Cell["\<\
where the unit vectors come outside the integrals because they are constant.\
\>", "Text",
 CellChangeTimes->{{3.6296152745252047`*^9, 3.629615281436017*^9}}],

Cell[TextData[{
 StyleBox["Example",
  FontVariations->{"Underline"->True}],
 "\nCalculate the volume integral of ",
 Cell[BoxData[
  FormBox[
   RowBox[{"T", "=", 
    RowBox[{"x", " ", "y", " ", 
     SuperscriptBox["z", "2"]}]}], TraditionalForm]]],
 " over the prism shown below."
}], "Text",
 CellChangeTimes->{{3.629615282671419*^9, 3.629615333199309*^9}}],

