Cell[TextData[{
 StyleBox["Solution",
  FontVariations->{"Underline"->True}],
 "\nWe proceed by induction. When ",
 Cell[BoxData[
  FormBox[
   RowBox[{"N", "=", "1"}], TraditionalForm]],ExpressionUUID->
  "d642fb93-4774-4096-98a1-b3c97e13b0b4"],
 ", ",
