Cell[TextData[{
 StyleBox["Solution",
  FontVariations->{"Underline"->True}],
 "\n",
 StyleBox["Method 1:",
  FontWeight->"Bold"],
 " Suppose ",
 Cell[BoxData[
  FormBox["C", TraditionalForm]],ExpressionUUID->
