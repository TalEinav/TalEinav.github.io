  FormBox[
   RowBox[{
    SubscriptBox["v", "A"], "=", 
    RowBox[{
     FractionBox["4", "5"], "c"}]}], TraditionalForm]],ExpressionUUID->
  "5e33a95c-1ead-4b9e-bc3c-29d9ad2ace1b"],
 " and ",
