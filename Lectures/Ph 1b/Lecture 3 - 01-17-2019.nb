Cell[TextData[{
 "We find the two solutions ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"v", "=", 
     RowBox[{
      FractionBox["5", "7"], "c"}]}], ",", 
    RowBox[{
     FractionBox["7", "5"], "c"}]}], TraditionalForm]],ExpressionUUID->
  "cd67ef4b-98ba-4b15-9fb8-54f4963133d9"],
