 Cell[BoxData[
  FormBox["A", TraditionalForm]],ExpressionUUID->
  "54a3025b-9609-42fa-8b1d-0b38b8b9a81f"],
 "\[CloseCurlyQuote]s velocity in ",
 Cell[BoxData[
  FormBox["C", TraditionalForm]],ExpressionUUID->
  "b8b8d9a6-0c0e-4dae-aa06-b422f403d1bf"],
 "\[CloseCurlyQuote]s frame would equal ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{
     RowBox[{
      FractionBox["4", "5"], "c"}], "-", "v"}], 
    RowBox[{"1", "-", 
     FractionBox[
      RowBox[{
       FractionBox["4", "5"], "c", " ", "v"}], 
      SuperscriptBox["c", "2"]]}]], TraditionalForm]],ExpressionUUID->
  "15d106c1-2b53-4c4f-b312-024b51c0002d"],
