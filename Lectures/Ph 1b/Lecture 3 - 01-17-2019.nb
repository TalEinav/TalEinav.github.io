2dc889ef5011"],

Cell[TextData[{
 "But ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[CapitalDelta]", "\[InvisibleComma]", "x"}], TraditionalForm]],
  ExpressionUUID->"8bb1007b-0fef-4e01-a639-9b7faba6f4c5"],
 " is by definition the length in ",
 Cell[BoxData[
  FormBox["S", TraditionalForm]],ExpressionUUID->
  "9daf9a12-0140-406e-80b8-d112f1431904"],
 ". And ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[CapitalDelta]", "\[InvisibleComma]", 
    RowBox[{"x", "'"}]}], TraditionalForm]],ExpressionUUID->
  "84e3b9be-dbc6-437e-9bab-133c0addd5ac"],
 " is the length in ",
 Cell[BoxData[
  FormBox[
   RowBox[{"S", "'"}], TraditionalForm]],ExpressionUUID->
  "eeb62054-e302-4adf-bf9c-ff77db13d16c"],
 ", because the stick is not moving in ",
 Cell[BoxData[
  FormBox[
   RowBox[{"S", "'"}], TraditionalForm]],ExpressionUUID->
  "934614a5-2766-4086-a162-c12acf594eb9"],
 ". Therefore, ",
 Cell[BoxData[
  FormBox[
   RowBox[{"l", "=", 
    FractionBox[
     RowBox[{"l", "'"}], "\[Gamma]"]}], TraditionalForm]],ExpressionUUID->
  "e4e0a0b8-6c33-4f13-b3ec-092af1e076fb"],
 ". Since ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Gamma]", "\[GreaterEqual]", "1"}], TraditionalForm]],
  ExpressionUUID->"b1460446-0b62-4441-8c68-37d02aa771e7"],
 ", we have ",
 Cell[BoxData[
  FormBox[
   RowBox[{"l", "\[LessEqual]", 
    RowBox[{"l", "'"}]}], TraditionalForm]],ExpressionUUID->
  "4903fe94-00d2-4119-82b1-ba111c569b8b"],
 ", so ",
 Cell[BoxData[
  FormBox["S", TraditionalForm]],ExpressionUUID->
  "c4e1c1ef-7958-4936-8d36-c3b01d230900"],
 " sees the stick shorter than ",
 Cell[BoxData[
  FormBox[
   RowBox[{"S", "'"}], TraditionalForm]],ExpressionUUID->
  "a88361f2-8239-44f8-bae5-81b6df0c0eb2"],
 " sees it."
}], "Text",
 CellChangeTimes->{{3.693108618621438*^9, 3.6931086212998123`*^9}, {
  3.6931087197484713`*^9, 3.6931087221861105`*^9}, {3.6931087559025283`*^9, 
  3.693108777114312*^9}, {3.693108886209996*^9, 3.693108920267167*^9}, {
