Cell[CellGroupData[{

Cell[TextData[{
 "Prevent the display of (long) results by using a semicolon at the end of \
input: ",
 StyleBox["expression", "TI"],
 StyleBox[";", "MR"],
 " "
}], "Item",
 CellChangeTimes->{{3.6229226178162165`*^9, 3.6229226867963395`*^9}}],

Cell[TextData[{
 "Independent inputs can either be placed on separate lines or they can be \
separated by semicolons: ",
 Cell[BoxData[
  FormBox[
   SubscriptBox[
    StyleBox["inputStatement", "TI"], "1"], TraditionalForm]]],
 StyleBox[";", "MR"],
 " ",
 Cell[BoxData[
  FormBox[
   SubscriptBox[
    StyleBox["inputStatement", "TI"], "2"], TraditionalForm]]],
 StyleBox[";", "MR"],
 " \[Ellipsis]",
 StyleBox[";", "MR"],
 " ",
 Cell[BoxData[
  FormBox[
   SubscriptBox[
    StyleBox["inputStatement", "TI"], "n"], TraditionalForm]]]
}], "Item",
 CellChangeTimes->{{3.6229226178162165`*^9, 3.622922797184534*^9}}],

Cell[TextData[{
 "The last expression given by ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 ": ",
 StyleBox["%", "MR"],
 " "
}], "Item",
 CellChangeTimes->{{3.6229226178162165`*^9, 3.6229226890115433`*^9}}],

Cell[TextData[{
 "The next\[Hyphen]to\[Hyphen]last (penultimate) expression given by ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 ": ",
 StyleBox["%%", "MR"],
 " "
}], "Item",
 CellChangeTimes->{{3.6229226178162165`*^9, 3.622922696829157*^9}}],

Cell[TextData[{
 "The ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    StyleBox["i", "TI"], "th"], TraditionalForm]]],
