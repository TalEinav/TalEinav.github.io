Cell[CellGroupData[{

Cell[BoxData[
 StyleBox[
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"Echo", "[", 
      RowBox[{"#", ",", "\"\<Possible \!\(\*OverscriptBox[
StyleBox[\"B\",
FontSlant->\"Italic\"], \"\[RightVector]\"]\): \>\""}], "]"}], "&"}], "/@", 
    RowBox[{"(", 
     RowBox[{"Quiet", "@", 
      RowBox[{"FullSimplify", "[", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"Ax", "^", "2"}], "+", 
             RowBox[{"Ay", "^", "2"}]}], ")"}], "^", 
           RowBox[{"(", 
            RowBox[{"1", "/", "2"}], ")"}]}], " ", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"Cos", "[", "\[Theta]", "]"}], ",", 
            RowBox[{"Sin", "[", "\[Theta]", "]"}]}], "}"}]}], "/.", 
         RowBox[{
          RowBox[{"Solve", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"D", "[", 
              RowBox[{
               RowBox[{
                RowBox[{"Ax", " ", 
                 RowBox[{
                  RowBox[{"(", 
                   RowBox[{
                    RowBox[{"Ax", "^", "2"}], "+", 
                    RowBox[{"Ay", "^", "2"}]}], ")"}], "^", 
                  RowBox[{"(", 
                   RowBox[{"1", "/", "2"}], ")"}]}], " ", 
                 RowBox[{"Sin", "[", "\[Theta]", "]"}]}], "-", 
                RowBox[{"Ay", " ", 
                 RowBox[{
                  RowBox[{"(", 
                   RowBox[{
                    RowBox[{"Ax", "^", "2"}], "+", 
                    RowBox[{"Ay", "^", "2"}]}], ")"}], "^", 
                  RowBox[{"(", 
                   RowBox[{"1", "/", "2"}], ")"}]}], " ", 
                 RowBox[{"Cos", "[", "\[Theta]", "]"}]}]}], ",", "\[Theta]"}],
               "]"}], "\[Equal]", "0"}], ",", "\[Theta]"}], "]"}], "[", 
          RowBox[{"[", 
           RowBox[{"2", ";;", "3"}], "]"}], "]"}]}], ",", 
        RowBox[{
         RowBox[{"0", "<", "Ax"}], "&&", 
         RowBox[{"0", "<", "Ay"}]}]}], "]"}]}], ")"}]}], ";"}],
  FontSize->10]], "Input",
 CellChangeTimes->{{3.6968278578972783`*^9, 3.6968278867747965`*^9}, {
  3.6968279330820665`*^9, 3.6968279369986725`*^9}, {3.714956404334453*^9, 
