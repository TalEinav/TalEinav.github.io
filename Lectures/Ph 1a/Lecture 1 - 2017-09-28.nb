Cell[TextData[{
 "Therefore, we find as before the two possibilities ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["B", "\[RightVector]"], "=", 
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{
      RowBox[{"-", 
       SubscriptBox["A", "y"]}], ",", 
      SubscriptBox["A", "x"]}], "\[RightAngleBracket]"}]}], TraditionalForm]],
  ExpressionUUID->"1f70fc02-2197-47ee-b3c0-f62d49a17a2c"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["B", "\[RightVector]"], "=", 
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{
      SubscriptBox["A", "y"], ",", 
      RowBox[{"-", 
       SubscriptBox["A", "x"]}]}], "\[RightAngleBracket]"}]}], 
   TraditionalForm]],ExpressionUUID->"bfe1fd58-37f8-4208-877e-5dc6b227e412"],
