Cell["The Taylor series of the Sin and Cos functions are", "Text",
 CellChangeTimes->{{3.714801947612602*^9, 
  3.7148019586570177`*^9}},ExpressionUUID->"c904e812-cc65-4517-8260-\
176826e5222f"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"Sin", "[", "x", "]"}], "=", 
    RowBox[{
     RowBox[{"x", "-", 
      FractionBox[
       SuperscriptBox["x", "3"], 
       RowBox[{"3", "!"}]], "+", 
      FractionBox[
       SuperscriptBox["x", "5"], 
       RowBox[{"5", "!"}]], "-", 
      RowBox[{"\[CenterDot]", "\[CenterDot]", "\[CenterDot]"}]}], "=", 
     RowBox[{
      RowBox[{
       SubsuperscriptBox["\[Sum]", 
        RowBox[{"j", "=", "0"}], "\[Infinity]"], 
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"-", "1"}], ")"}], "j"], 
        FractionBox[
         SuperscriptBox["x", 
          RowBox[{
           RowBox[{"2", "j"}], "+", "1"}]], 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"2", "j"}], "+", "1"}], ")"}], "!"}]]}]}], "=", 
      FractionBox[
       RowBox[{
        SuperscriptBox["\[ExponentialE]", 
         RowBox[{"\[ImaginaryI]", " ", "x"}]], "-", 
        SuperscriptBox["\[ExponentialE]", 
         RowBox[{
          RowBox[{"-", "\[ImaginaryI]"}], " ", "x"}]]}], 
       RowBox[{"2", "\[ImaginaryI]"}]]}]}]}], TraditionalForm]],
  ExpressionUUID->"16767589-99ed-4c45-9f59-d16a283fb41f"],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"Cos", "[", "x", "]"}], "=", 
    RowBox[{
     RowBox[{"1", "-", 
      FractionBox[
       SuperscriptBox["x", "2"], 
       RowBox[{"2", "!"}]], "+", 
      FractionBox[
       SuperscriptBox["x", "4"], 
       RowBox[{"4", "!"}]], "-", 
      RowBox[{"\[CenterDot]", "\[CenterDot]", "\[CenterDot]"}]}], "=", 
     RowBox[{
      RowBox[{
       SubsuperscriptBox["\[Sum]", 
        RowBox[{"j", "=", "0"}], "\[Infinity]"], 
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"-", "1"}], ")"}], "j"], 
        FractionBox[
         SuperscriptBox["x", 
          RowBox[{"2", "j"}]], 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"2", "j"}], ")"}], "!"}]]}]}], "=", 
      FractionBox[
       RowBox[{
        SuperscriptBox["\[ExponentialE]", 
         RowBox[{"\[ImaginaryI]", " ", "x"}]], "+", 
        SuperscriptBox["\[ExponentialE]", 
         RowBox[{
          RowBox[{"-", "\[ImaginaryI]"}], " ", "x"}]]}], "2"]}]}]}], 
   TraditionalForm]],ExpressionUUID->"c438e0e0-ea26-4c70-8cb6-de3e0b6ddbdb"]
}], "TextNumbered",
 CellChangeTimes->{{3.620951619395096*^9, 3.6209516274165545`*^9}, {
  3.6209517415630836`*^9, 3.6209517659744797`*^9}, {3.620951813936223*^9, 
