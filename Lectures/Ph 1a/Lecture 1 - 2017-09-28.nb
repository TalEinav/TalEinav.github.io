Cell["\<\
In Cartesian coordinates, this problem is both difficult to set up (because \
the integration bounds are awkward) and difficult to evaluate. Nevertheless, \
what doesn\[CloseCurlyQuote]t kill you makes you stronger.\
\>", "Text",
 CellChangeTimes->{{3.7146266567296224`*^9, 
  3.714626700758339*^9}},ExpressionUUID->"03ff6004-db9c-42fe-a244-\
e4d1b2586186"],

Cell[TextData[Cell[BoxData[
 FormBox[GridBox[{
    {GridBox[{
       {
        RowBox[{
         RowBox[{"Circle", " ", "Area"}], "=", 
         RowBox[{
          SubsuperscriptBox["\[Integral]", 
           RowBox[{"-", "R"}], "R"], 
          RowBox[{
           SubsuperscriptBox["\[Integral]", 
            RowBox[{"-", 
             SuperscriptBox[
              RowBox[{"(", 
               RowBox[{
                SuperscriptBox["R", "2"], "-", 
                SuperscriptBox["y", "2"]}], ")"}], 
              RowBox[{"1", "/", "2"}]]}], 
            SuperscriptBox[
             RowBox[{"(", 
              RowBox[{
               SuperscriptBox["R", "2"], "-", 
               SuperscriptBox["y", "2"]}], ")"}], 
             RowBox[{"1", "/", "2"}]]], 
           RowBox[{
            RowBox[{"\[DifferentialD]", "x"}], 
            RowBox[{"\[DifferentialD]", "y"}]}]}]}]}]},
       {
        RowBox[{"=", 
         RowBox[{
          SubsuperscriptBox["\[Integral]", 
           RowBox[{"-", "R"}], "R"], 
          RowBox[{"2", 
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{
              SuperscriptBox["R", "2"], "-", 
              SuperscriptBox["y", "2"]}], ")"}], 
            RowBox[{"1", "/", "2"}]], 
           RowBox[{"\[DifferentialD]", "y"}]}]}]}]},
       {
        RowBox[{"=", 
         SubsuperscriptBox[
          RowBox[{"(", 
           RowBox[{
            RowBox[{"y", " ", 
             SuperscriptBox[
              RowBox[{"(", 
               RowBox[{
                SuperscriptBox["R", "2"], "-", 
                SuperscriptBox["y", "2"]}], ")"}], 
              RowBox[{"1", "/", "2"}]]}], "+", 
            RowBox[{
             SuperscriptBox["R", "2"], 
             RowBox[{"ArcTan", "[", 
              FractionBox["y", 
               SuperscriptBox[
                RowBox[{"(", 
                 RowBox[{
                  SuperscriptBox["R", "2"], "-", 
                  SuperscriptBox["y", "2"]}], ")"}], 
                RowBox[{"1", "/", "2"}]]], "]"}]}]}], ")"}], 
          RowBox[{"y", "=", 
           RowBox[{"-", "R"}]}], 
          RowBox[{"y", "=", "R"}]]}]},
       {
        RowBox[{"=", 
         RowBox[{
          RowBox[{
           FractionBox["\[Pi]", "2"], " ", 
           SuperscriptBox["R", "2"]}], "-", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"-", 
             FractionBox["\[Pi]", "2"]}], " ", 
            SuperscriptBox["R", "2"]}], ")"}]}]}]},
       {
        RowBox[{"=", 
         RowBox[{"\[Pi]", " ", 
          SuperscriptBox["R", "2"]}]}]}
      },
      GridBoxAlignment->{"Columns" -> {{"="}}}]}
   },
