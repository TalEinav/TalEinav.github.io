Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"f", "[", "x", "]"}], "=", 
   RowBox[{
    RowBox[{"f", "[", "0", "]"}], "+", 
    RowBox[{
     FractionBox[
      RowBox[{
       RowBox[{"f", "'"}], "[", "0", "]"}], 
      RowBox[{"1", "!"}]], "x"}], "+", 
    RowBox[{
     FractionBox[
      RowBox[{
       RowBox[{"f", "''"}], "[", "0", "]"}], 
      RowBox[{"2", "!"}]], 
     SuperscriptBox["x", "2"]}], "+", 
    RowBox[{
     FractionBox[
      RowBox[{
       RowBox[{"f", "'''"}], "[", "0", "]"}], 
      RowBox[{"3", "!"}]], 
     SuperscriptBox["x", "3"]}], "+", 
    RowBox[{"\[CenterDot]", "\[CenterDot]", "\[CenterDot]"}]}]}], 
