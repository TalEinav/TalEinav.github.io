Cell[TextData[{
 StyleBox["Example",
  FontVariations->{"Underline"->True}],
 "\nConvert the three points with Cartesian coordinates ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["p", "1"], "=", 
    RowBox[{"(", 
     RowBox[{"1", ",", "0", ",", "0"}], ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "7c43ea76-7a6c-4e88-8fd2-1b35f483e2b6"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["p", "2"], "=", 
    RowBox[{"(", 
     RowBox[{"1", ",", "1", ",", "0"}], ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "8056a3e2-dd51-4e18-9b26-6484a201ecd5"],
 ", and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["p", "3"], "=", 
    RowBox[{"(", 
     RowBox[{"0", ",", "0", ",", "1"}], ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "37464a84-e3a4-4357-8b74-de59dc1ac4cb"],
 " into cylindrical and spherical coordinates."
}], "Text",
 CellChangeTimes->{{3.7146250257458324`*^9, 3.71462507454101*^9}, 
   3.7146254632002034`*^9},ExpressionUUID->"3c6ddb6a-5d3e-4f98-984d-\
61f689c2381f"],

Cell[TextData[{
 StyleBox["Solution",
  FontVariations->{"Underline"->True}],
 "\nWe start with spherical coordinates. The three points are sufficiently \
nice (either falling on an axis or symmetrically between two axes) that you \
can infer ",
 Cell[BoxData[
  FormBox["r", TraditionalForm]],ExpressionUUID->
  "d91bce6e-9904-4f42-959a-55775235317e"],
 ", \[Theta], and \[Phi] by eye using the diagram above. Then you can double \
check your work by using Equations (",
 Cell[BoxData[
  FormBox[
   CounterBox["TextNumbered", "eq1"], TraditionalForm]],ExpressionUUID->
  "b19cb139-eec5-4748-bc5a-8a95bc841fae"],
 ")-(",
 Cell[BoxData[
  FormBox[
   CounterBox["TextNumbered", "eq2"], TraditionalForm]],ExpressionUUID->
  "10058116-1734-4caf-9724-f2b6d00396e5"],
 "). In either case, you should obtain"
}], "Text",
 CellChangeTimes->{{3.7146250257458324`*^9, 3.7146250758579645`*^9}, {
   3.714625156850996*^9, 3.7146253183423605`*^9}, 
   3.7149594843327336`*^9},ExpressionUUID->"b006884f-6684-4e88-941f-\
e9d863eea46f"]
}, Open  ]],

