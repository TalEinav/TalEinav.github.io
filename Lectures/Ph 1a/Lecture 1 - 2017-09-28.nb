Cell[CellGroupData[{

Cell["\<\
In this section, we work in 3D Cartesian coordinates because the cross \
product is not defined in 2D.\
\>", "Text",
 CellChangeTimes->{
  3.620956032196494*^9, {3.620956137680527*^9, 3.6209561817930503`*^9}, {
   3.620956887856435*^9, 3.6209569132898893`*^9}, 
   3.620973429350168*^9},ExpressionUUID->"53969d48-d659-4f9e-b191-\
33b71084d8b1"],

Cell[TextData[{
 "Given two vectors ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["A", "\[RightVector]"], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["A", "x"], 
      OverscriptBox[
       StyleBox["x",
        FontWeight->"Plain"], "^"]}], "+", 
     RowBox[{
      SubscriptBox["A", "y"], 
      OverscriptBox[
       StyleBox["y",
        FontWeight->"Plain"], "^"]}], "+", 
     RowBox[{
      SubscriptBox["A", "z"], 
      OverscriptBox[
       StyleBox["z",
        FontWeight->"Plain"], "^"]}]}]}], TraditionalForm]],ExpressionUUID->
  "5a12b2dd-29fc-4ce5-8291-b2e04194abff"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["B", "\[RightVector]"], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["B", "x"], 
      OverscriptBox[
       StyleBox["x",
        FontWeight->"Plain"], "^"]}], "+", 
     RowBox[{
      SubscriptBox["B", "y"], 
      OverscriptBox[
       StyleBox["y",
        FontWeight->"Plain"], "^"]}], "+", 
     RowBox[{
      SubscriptBox["B", "z"], 
      OverscriptBox[
       StyleBox["z",
        FontWeight->"Plain"], "^"]}]}]}], TraditionalForm]],ExpressionUUID->
  "f2f2eb12-9c40-4bfa-8548-09ec20e7f273"],
 ", we define the ",
 StyleBox["cross product",
  FontSlant->"Italic",
  FontVariations->{"Underline"->True}],
 " of ",
 Cell[BoxData[
  FormBox[
   OverscriptBox["A", "\[RightVector]"], TraditionalForm]],ExpressionUUID->
  "a9efd39b-0833-4e54-b3a8-adb3eb78f1cb"],
 " and ",
 Cell[BoxData[
  FormBox[
   OverscriptBox["B", "\[RightVector]"], TraditionalForm]],ExpressionUUID->
  "f6798d4d-dd10-4c54-a440-011e38a621e1"],
 " as"
}], "Text",
 CellChangeTimes->{
  3.620956032196494*^9, {3.620956137680527*^9, 3.6209561946487856`*^9}, {
   3.621041702528968*^9, 3.6210417189749084`*^9}, {3.714618763977739*^9, 
   3.7146187645521526`*^9}, {3.714618796552022*^9, 
   3.714618802129815*^9}},ExpressionUUID->"6bc22dbc-25c3-4973-bfb4-\
b31732c84e82"]
}, Open  ]],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    OverscriptBox["A", "\[RightVector]"], 
    StyleBox["\[Cross]",
     FontWeight->"Plain"], 
    OverscriptBox["B", "\[RightVector]"]}], 
   StyleBox["\[Congruent]",
    FontWeight->"Plain"], 
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        SubscriptBox[
         StyleBox["A",
          FontWeight->"Plain"], "y"], 
        SubscriptBox["B", "z"]}], "-", 
       RowBox[{
        SubscriptBox["A", "z"], 
        SubscriptBox["B", "y"]}]}], ")"}], 
     OverscriptBox[
      StyleBox["x",
       FontWeight->"Plain"], "^"]}], "+", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{
