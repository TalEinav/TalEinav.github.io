  FormBox[
   RowBox[{"\[Theta]", "=", 
    FractionBox["\[Pi]", "2"]}], TraditionalForm]],ExpressionUUID->
  "6a3492be-6b10-4209-b7a6-7cf62a1de729"],
 ":"
}], "Text",
 CellChangeTimes->{{3.623272102948811*^9, 3.6232721038588123`*^9}, {
   3.6232721377498617`*^9, 3.623272139994866*^9}, {3.623280430798417*^9, 
   3.623280432120493*^9}, {3.623280468219558*^9, 3.6232804969592013`*^9}, {
