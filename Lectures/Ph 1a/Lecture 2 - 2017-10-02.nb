Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox[
    OverscriptBox["v", "\[RightVector]"], "rot"], "=", 
   RowBox[{
    RowBox[{
     OverscriptBox["v", "\[RightVector]"], " ", 
     RowBox[{"Cos", "[", "\[Theta]", "]"}]}], "+", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       OverscriptBox[
        StyleBox["z",
         FontWeight->"Plain"], "^"], "\[Cross]", 
       OverscriptBox["v", "\[RightVector]"]}], ")"}], 
     RowBox[{"Sin", "[", "\[Theta]", "]"}]}], "+", 
    RowBox[{
     OverscriptBox[
      StyleBox["z",
       FontWeight->"Plain"], "^"], " ", 
     RowBox[{"(", 
      RowBox[{
       OverscriptBox[
        StyleBox["z",
         FontWeight->"Plain"], "^"], "\[CenterDot]", 
       OverscriptBox["v", "\[RightVector]"]}], ")"}], 
     RowBox[{"(", 
      RowBox[{"1", "-", 
       RowBox[{"Cos", "[", "\[Theta]", "]"}]}], ")"}]}]}]}], 
  TraditionalForm]],ExpressionUUID->"e806f882-8fdd-4723-852e-cfc93455120c"]], \
"TextNumbered",
