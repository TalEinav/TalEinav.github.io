Cell[TextData[{
 "Thus ",
 Cell[BoxData[
  FormBox[
   RowBox[{"N", "=", 
    RowBox[{"M", " ", "g", " ", 
     RowBox[{"Cos", "[", "\[Theta]", "]"}]}]}], TraditionalForm]],
  ExpressionUUID->"c643b3cc-5d32-4cae-8e86-29c6be627404"],
 " while ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["F", "friction"], "=", 
    RowBox[{"M", " ", "g", " ", 
     RowBox[{"Sin", "[", "\[Theta]", "]"}]}]}], TraditionalForm]],
  ExpressionUUID->"a363e35d-074b-476e-8924-a8622bf3155e"],
 ". However, to remain in static equilibrium, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["F", "friction"], "\[LessEqual]", 
     RowBox[{"\[Mu]", " ", "N"}]}], "=", 
    RowBox[{"\[Mu]", " ", "M", " ", "g", " ", 
     RowBox[{"Cos", "[", "\[Theta]", "]"}]}]}], TraditionalForm]],
  ExpressionUUID->"04b2f098-b7b3-436f-aaad-fbc8f68c9c52"],
 ". Therefore, "
