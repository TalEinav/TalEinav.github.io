Cell[TextData[{
 "This question is straightforward if we realize that cylindrical coordinates \
are equivalent to 2D polar coordinates with the Cartesian ",
 Cell[BoxData[
  FormBox["z", TraditionalForm]],ExpressionUUID->
  "e410ca3f-341a-48c9-a6cd-9c0e3854dd50"],
 "-coordinate tacked on. This implies that the cylindrical coordinate unit \
vectors are given by Equations (",
 Cell[BoxData[
  FormBox[
   CounterBox["TextNumbered", "eq1"], TraditionalForm]],ExpressionUUID->
  "9cd4fc55-28d5-485f-951c-3d0f87078dc6"],
 ") and (",
 Cell[BoxData[
  FormBox[
   CounterBox["TextNumbered", "eq2"], TraditionalForm]],ExpressionUUID->
  "5981ab11-3872-4c54-b5c9-5eb71b50fd77"],
 ") as"
}], "Text",
 CellChangeTimes->{{3.699492726405168*^9, 3.6994927872988615`*^9}, {
  3.69949281968032*^9, 3.6994928454913807`*^9}, {3.7149666979212217`*^9, 
  3.714966699785614*^9}},ExpressionUUID->"b0fdbf27-15b0-4e78-a47a-\
bf28ff20f43f"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   OverscriptBox["r", "^"], 
   StyleBox["=",
    FontWeight->"Plain"], 
   RowBox[{
    RowBox[{
     RowBox[{
      StyleBox["Cos",
       FontWeight->"Plain"], "[", "\[Theta]", "]"}], 
     OverscriptBox["x", "^"]}], "+", 
    RowBox[{
     RowBox[{
      StyleBox["Sin",
       FontWeight->"Plain"], "[", "\[Theta]", "]"}], 
     OverscriptBox["y", "^"]}]}]}], 
  TraditionalForm]],ExpressionUUID->"d8b650a2-477c-4743-8c39-a91b307407d2"]], \
"TextNumbered",
 CellChangeTimes->{{3.6994909269713287`*^9, 3.6994909303743353`*^9}, {
  3.69949280602684*^9, 3.6994928081251*^9}, {3.7149658866206565`*^9, 
  3.7149658866206565`*^9}, {3.7149660340215015`*^9, 3.7149660340215015`*^9}, {
  3.7149667103431625`*^9, 3.7149667140568285`*^9}},
 CellTags->"eq1",ExpressionUUID->"37dcfcfc-d287-4121-be97-fac75714af60"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   OverscriptBox["\[Theta]", "^"], 
   StyleBox["=",
    FontWeight->"Plain"], 
   RowBox[{
    RowBox[{
     RowBox[{
      StyleBox["-",
       FontWeight->"Plain"], 
      RowBox[{
       StyleBox["Sin",
        FontWeight->"Plain"], "[", "\[Theta]", "]"}]}], 
     OverscriptBox["x", "^"]}], "+", 
    RowBox[{
     RowBox[{
      StyleBox["Cos",
       FontWeight->"Plain"], "[", "\[Theta]", "]"}], 
     OverscriptBox["y", "^"]}]}]}], 
  TraditionalForm]],ExpressionUUID->"318115ad-3b40-4e50-848b-5851b441bc39"]], \
"TextNumbered",
 CellChangeTimes->{{3.6209670047750893`*^9, 3.620967023756175*^9}, {
  3.6209670684857335`*^9, 3.620967073230005*^9}, {3.6994928107319403`*^9, 
  3.6994928135052195`*^9}, {3.714965887173061*^9, 3.714965887173061*^9}, {
  3.7149660343890657`*^9, 3.7149660343890657`*^9}, {3.7149667078897114`*^9, 
  3.7149667165597105`*^9}},
 CellTags->"eq2",ExpressionUUID->"fba37c88-4d05-471f-986f-36436047bcc9"],

Cell[TextData[{
 "Meanwhile, the ",
 Cell[BoxData[
  FormBox["z", TraditionalForm]],ExpressionUUID->
  "84759b0e-b6ff-4632-a330-7b35360fc7bb"],
 "-coordinate is identical to the Cartesian ",
 Cell[BoxData[
  FormBox["z", TraditionalForm]],ExpressionUUID->
