Cell[TextData[{
 "Note that the unit vectors are all orthogonal to each other, and that the \
coordinate system is right handed. In the language of the cross product, this \
implies that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     OverscriptBox["r", "^"], 
     StyleBox["\[Cross]",
      FontWeight->"Plain"], 
     OverscriptBox["\[Theta]", "^"]}], "=", 
    OverscriptBox["\[Phi]", "^"]}], TraditionalForm]],ExpressionUUID->
  "ef865e41-83ad-4fc4-ae06-96c730f2fed0"],
 " (similar to how in Cartesian coordinates ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     OverscriptBox["x", "^"], 
     StyleBox["\[Cross]",
      FontWeight->"Plain"], 
     OverscriptBox["y", "^"]}], "=", 
    OverscriptBox["z", "^"]}], TraditionalForm]],ExpressionUUID->
  "5e4557fa-bfc9-47b9-af9b-3387ae9712f2"],
 ")."
}], "Text",
 CellChangeTimes->{{3.699492189399597*^9, 3.699492268097769*^9}, {
  3.714965882635966*^9, 3.714965882635966*^9}, {3.714966031212743*^9, 
  3.714966046365568*^9}},ExpressionUUID->"39169add-6214-46fa-bb6f-\
82f77a0820d9"],

Cell["\<\
As a fun exercise, you can show that the spherical coordinate unit vectors \
are related to the Cartesian coordinate vectors by\
\>", "Text",
 CellChangeTimes->{{3.699492289484016*^9, 3.6994923047521043`*^9}}],

Cell[TextData[Cell[BoxData[
 FormBox[GridBox[{
    {GridBox[{
       {
        RowBox[{
         OverscriptBox["r", "^"], "=", 
         RowBox[{
          RowBox[{
           RowBox[{"Sin", "[", "\[Theta]", "]"}], 
           RowBox[{"Cos", "[", "\[Phi]", "]"}], 
           OverscriptBox["x", "^"]}], "+", 
          RowBox[{
           RowBox[{"Sin", "[", "\[Theta]", "]"}], 
           RowBox[{"Sin", "[", "\[Phi]", "]"}], 
           OverscriptBox["y", "^"]}], "+", 
          RowBox[{
           RowBox[{"Cos", "[", "\[Theta]", "]"}], 
           OverscriptBox["z", "^"]}]}]}]},
       {
        RowBox[{
         OverscriptBox["\[Theta]", "^"], "=", 
         RowBox[{
          RowBox[{
           RowBox[{"Cos", "[", "\[Theta]", "]"}], 
           RowBox[{"Cos", "[", "\[Phi]", "]"}], 
           OverscriptBox["x", "^"]}], "+", 
          RowBox[{
           RowBox[{"Cos", "[", "\[Theta]", "]"}], 
           RowBox[{"Sin", "[", "\[Phi]", "]"}], 
           OverscriptBox["y", "^"]}], "-", 
          RowBox[{
           RowBox[{"Sin", "[", "\[Theta]", "]"}], 
           OverscriptBox["z", "^"]}]}]}]},
       {
        RowBox[{
         OverscriptBox["\[Phi]", "^"], "=", 
         RowBox[{
          RowBox[{
           RowBox[{"-", 
            RowBox[{"Sin", "[", "\[Phi]", "]"}]}], 
           OverscriptBox["x", "^"]}], "+", 
          RowBox[{
           RowBox[{"Cos", "[", "\[Phi]", "]"}], 
           OverscriptBox["y", "^"]}]}]}]}
      },
      GridBoxAlignment->{"Columns" -> {{"="}}}]}
   },
   GridBoxItemSize->{"Columns" -> {{
        Scaled[0.96]}}}], 
