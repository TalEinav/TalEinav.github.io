  RowBox[{
   FractionBox[
    SubscriptBox["N", "1"], "F"], "=", 
   RowBox[{
    FractionBox[
     RowBox[{"d", " ", 
      RowBox[{"Sin", "[", "\[Theta]", "]"}]}], 
     RowBox[{
      FractionBox["d", "2"], 
      RowBox[{"Cos", "[", "\[Theta]", "]"}]}]], "=", 
