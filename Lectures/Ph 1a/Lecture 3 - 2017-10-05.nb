ec6beb3ab071"],

Cell[TextData[{
 "Using a Taylor series, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"T", "[", 
     RowBox[{"\[Theta]", "+", 
      RowBox[{"\[DifferentialD]", "\[Theta]"}]}], "]"}], "=", 
    RowBox[{
     RowBox[{"T", "[", "\[Theta]", "]"}], "+", 
     RowBox[{
      RowBox[{
       RowBox[{"T", "'"}], "[", "\[Theta]", "]"}], 
      RowBox[{"\[DifferentialD]", "\[Theta]"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"3111b090-254f-465c-93b3-34b2f29adb98"],
 ", "
}], "Text",
