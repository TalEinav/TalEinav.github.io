Cell[TextData[{
 "Cupcake A hits the ground when its ",
 Cell[BoxData[
  FormBox["y", TraditionalForm]],ExpressionUUID->
  "d468f3bc-ddb9-4085-b316-903f08c19619"],
 "-component is zero, "
}], "Text",
