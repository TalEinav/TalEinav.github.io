 FormBox[
  RowBox[{
   RowBox[{"\[LeftBracketingBar]", 
    RowBox[{
     SubscriptBox[
      OverscriptBox["r", "\[RightVector]"], "A"], "-", 
     SubscriptBox[
      OverscriptBox["r", "\[RightVector]"], "B"]}], "\[RightBracketingBar]"}],
    "=", 
   SqrtBox[
    RowBox[{
     SuperscriptBox[
      RowBox[{"{", 
