 FormBox[
  RowBox[{"0", "=", 
   RowBox[{"d", "-", 
    RowBox[{"v", " ", 
     RowBox[{"(", 
      RowBox[{
       FractionBox[
        RowBox[{"2", "v"}], "g"], " ", 
       RowBox[{"Sin", "[", 
        SubscriptBox["\[Theta]", "B"], "]"}]}], ")"}], " ", 
     RowBox[{"Cos", "[", 
      SubscriptBox["\[Theta]", "B"], "]"}]}]}]}], 
