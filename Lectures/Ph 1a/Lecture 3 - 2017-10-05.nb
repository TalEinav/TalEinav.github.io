11addd91b7f6"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{"0", "=", 
   RowBox[{
    RowBox[{"v", " ", "t", " ", 
     RowBox[{"Sin", "[", 
      SubscriptBox["\[Theta]", "A"], "]"}]}], "-", 
    RowBox[{
     FractionBox["1", "2"], "g", " ", 
     SuperscriptBox["t", "2"]}]}]}], 
  TraditionalForm]],ExpressionUUID->"abbd1aba-b735-4307-9c9c-502732882c3e"]], \
