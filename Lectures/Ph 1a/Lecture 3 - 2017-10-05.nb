   RowBox[{"T", "[", "\[Theta]", "]"}], "\[LessEqual]", 
   RowBox[{
    SubscriptBox["T", "0"], 
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{"\[Mu]", " ", "\[Theta]"}]]}]}], 
  TraditionalForm]],ExpressionUUID->"16c2ae5a-5815-4ca6-b769-8c8898e9f3ff"]], \
