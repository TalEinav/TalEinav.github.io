Cell["\<\
we can integrate over the top-half of the ellipse in Cartesian coordinates \
(multiplying by 2 to account for the bottom half)\
\>", "Text",
 CellChangeTimes->{{3.6330519509695234`*^9, 3.633051976621991*^9}, 
   3.633052444394746*^9}],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"2", 
    RowBox[{
     SubsuperscriptBox["\[Integral]", 
      RowBox[{"-", "a"}], "a"], " ", 
     SuperscriptBox[
      RowBox[{"b", "(", 
       RowBox[{"1", "-", 
        FractionBox[
         SuperscriptBox["x", "2"], 
         SuperscriptBox["a", "2"]]}], ")"}], 
      RowBox[{"1", "/", "2"}]]}]}], "=", 
   RowBox[{
    RowBox[{"b", " ", 
     SubsuperscriptBox[
      RowBox[{"{", 
       RowBox[{
        RowBox[{"x", " ", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"1", "-", 
            FractionBox[
             SuperscriptBox["x", "2"], 
             SuperscriptBox["a", "2"]]}], ")"}], 
          RowBox[{"1", "/", "2"}]]}], "+", 
        RowBox[{"a", " ", 
         RowBox[{"ArcSin", "[", 
          FractionBox["x", "a"], "]"}]}]}], "}"}], 
      RowBox[{"x", "=", 
       RowBox[{"-", "a"}]}], 
      RowBox[{"x", "=", "a"}]]}], "=", 
    RowBox[{
     RowBox[{"a", " ", "b", " ", 
      RowBox[{"(", 
       RowBox[{
        FractionBox["\[Pi]", "2"], "-", 
        RowBox[{"(", 
         RowBox[{"-", 
          FractionBox["\[Pi]", "2"]}], ")"}]}], ")"}]}], "=", 
     RowBox[{"\[Pi]", " ", "a", " ", "b"}]}]}]}], 
  TraditionalForm]]]], "TextNumbered",
 CellChangeTimes->{{3.6330519775590444`*^9, 3.633051989736741*^9}, {
