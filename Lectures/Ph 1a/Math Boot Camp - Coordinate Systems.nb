Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{"\[Theta]", "=", 
   RowBox[{
    RowBox[{"ArcTan", "[", 
     FractionBox[
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         SubsuperscriptBox["A", "x", "2"], "+", 
         SubsuperscriptBox["A", "y", "2"]}], ")"}], 
       RowBox[{"1", "/", "2"}]], 
      SubscriptBox["A", "z"]], "]"}], "=", 
    RowBox[{"ArcCos", "[", 
     FractionBox[
      SubscriptBox["A", "z"], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         SubsuperscriptBox["A", "x", "2"], "+", 
         SubsuperscriptBox["A", "y", "2"], "+", 
         SubsuperscriptBox["A", "z", "2"]}], ")"}], 
       RowBox[{"1", "/", "2"}]]], "]"}]}]}], 
  TraditionalForm]]]], "TextNumbered",
