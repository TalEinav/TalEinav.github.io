 FormBox[
  RowBox[{
   SubscriptBox["z", "max"], "=", 
   FractionBox[
    SuperscriptBox["v", "2"], 
    RowBox[{"2", " ", "g"}]]}], 
  TraditionalForm]],ExpressionUUID->"e5edc842-d5c2-4efb-adac-4c18c9582fdb"]], \
