Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   OverscriptBox["r", "^"], "=", 
   RowBox[{
    RowBox[{
     FractionBox["x", "r"], 
     OverscriptBox["x", "^"]}], "+", 
    RowBox[{
     FractionBox["y", "r"], 
     OverscriptBox["y", "^"]}], "+", 
    RowBox[{
     FractionBox["z", "r"], 
     OverscriptBox["z", "^"]}]}]}], TraditionalForm]]]], "TextNumbered",
 CellChangeTimes->{{3.6227725045984373`*^9, 3.6227725155370636`*^9}}],

Cell["\<\
which you can verify is both a unit vector and points in the radial \
direction, we can compute\
\>", "Text",
 CellChangeTimes->{{3.62277251830567*^9, 3.622772544891121*^9}}],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   FractionBox[
    RowBox[{"\[PartialD]", "r"}], 
    RowBox[{"\[PartialD]", "x"}]], "=", 
   RowBox[{
    FractionBox[
     RowBox[{"\[PartialD]", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox["x", "2"], "+", 
         SuperscriptBox["y", "2"], "+", 
         SuperscriptBox["z", "2"]}], ")"}], 
       RowBox[{"1", "/", "2"}]]}], 
     RowBox[{"\[PartialD]", "x"}]], "=", 
    FractionBox["x", "r"]}]}], TraditionalForm]]]], "TextNumbered",
 CellChangeTimes->{{3.622772546930125*^9, 3.6227725659323597`*^9}}],

Cell["and similarly", "Text",
 CellChangeTimes->{{3.6227725781524057`*^9, 3.622772579683609*^9}}],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   FractionBox[
