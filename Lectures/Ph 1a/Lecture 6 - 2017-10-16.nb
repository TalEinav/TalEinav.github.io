Cell[TextData[{
 "which means that every point in space would feel a rotation of the same \
magnitude characterized by an axis of rotation ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["\[Omega]", "\[RightVector]"], "=", 
    RowBox[{
     RowBox[{"-", "2"}], " ", 
     OverscriptBox["z", "^"]}]}], TraditionalForm]]],
 ". \[EmptySquare] "
}], "Text",
