Cell[TextData[Cell[BoxData[
 FormBox[GridBox[{
    {GridBox[{
       {
        RowBox[{
         RowBox[{"F", "[", "x", "]"}], "=", 
         RowBox[{"m", " ", "a"}]}]},
       {
        RowBox[{"=", 
         RowBox[{"m", 
          FractionBox[
           RowBox[{"\[DifferentialD]", "v"}], 
           RowBox[{"\[DifferentialD]", "t"}]]}]}]},
       {
        RowBox[{"=", 
         RowBox[{"m", " ", 
          FractionBox[
           RowBox[{"\[DifferentialD]", "v"}], 
           RowBox[{"\[DifferentialD]", "x"}]], 
          FractionBox[
           RowBox[{"\[DifferentialD]", "x"}], 
           RowBox[{"\[DifferentialD]", "t"}]]}]}]},
       {
        RowBox[{"=", 
         RowBox[{"m", " ", "v", " ", 
          FractionBox[
           RowBox[{"\[DifferentialD]", "v"}], 
           RowBox[{"\[DifferentialD]", "x"}]]}]}]}
      },
      GridBoxAlignment->{"Columns" -> {{"="}}}]}
   },
   GridBoxItemSize->{"Columns" -> {{
        Scaled[0.96]}}}], TraditionalForm]]]], "TextNumbered",
 CellChangeTimes->{{3.6227606084399595`*^9, 3.622760635220863*^9}}],

Cell[TextData[{
 "Multiplying both sides by ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[DifferentialD]", "x"}], TraditionalForm]]],
 " and integrating (say ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]]],
 " ranges from ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "0"], TraditionalForm]]],
 " to ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]]],
 " and ",
 Cell[BoxData[
  FormBox["v", TraditionalForm]]],
 " from ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["v", "0"], TraditionalForm]]],
 " to ",
 Cell[BoxData[
  FormBox["v", TraditionalForm]]],
 " (note that technically our integration variables should be ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "'"}], TraditionalForm]]],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"v", "'"}], TraditionalForm]]],
 ", but that notation is cumbersome)), "
}], "Text",
 CellChangeTimes->{{3.6227606471080837`*^9, 3.6227606547988973`*^9}, {
  3.6227607111681976`*^9, 3.622760742699853*^9}}],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    FractionBox["1", "2"], "m", " ", 
    SuperscriptBox["v", "2"]}], "=", 
   RowBox[{"E", "+", 
    RowBox[{
     SubsuperscriptBox["\[Integral]", 
      SubscriptBox["x", "0"], "x"], 
     RowBox[{
      RowBox[{"F", "[", "x", "]"}], 
      RowBox[{"\[DifferentialD]", "x"}]}]}]}]}], 
  TraditionalForm]]]], "TextNumbered",
 CellChangeTimes->{{3.6227606626491117`*^9, 3.622760694443968*^9}, {
  3.6227607628092885`*^9, 3.622760764712492*^9}}],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["E", TraditionalForm]],ExpressionUUID->
  "25eac2c7-8cdd-41b9-9b74-5151e5fafdd4"],
 " is a constant that depends on ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "0"], TraditionalForm]],ExpressionUUID->
  "801983df-3e81-4eeb-9173-c4f4be2066fb"],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["v", "0"], TraditionalForm]],ExpressionUUID->
  "b6c6d39d-673e-4f25-8b55-2b49f5995670"],
 ". Defining "
}], "Text",
 CellChangeTimes->{{3.6227607917337394`*^9, 
  3.6227608082093687`*^9}},ExpressionUUID->"5c77c241-2ac7-4162-a51a-\
828a2c2d5081"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"V", "[", "x", "]"}], "\[Congruent]", 
   RowBox[{"-", 
    RowBox[{
     SubsuperscriptBox["\[Integral]", 
      SubscriptBox["x", "0"], "x"], 
     RowBox[{
      RowBox[{"F", "[", "x", "]"}], 
      RowBox[{"\[DifferentialD]", "x"}]}]}]}]}], 
  TraditionalForm]]]], "TextNumbered",
 CellChangeTimes->{{3.6227608106897726`*^9, 3.6227608163369827`*^9}}],

Cell["we find", "Text",
 CellChangeTimes->{{3.622760818193386*^9, 3.622760819255188*^9}}],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{
     FractionBox["1", "2"], "m", " ", 
     SuperscriptBox["v", "2"]}], "+", 
    RowBox[{"V", "[", "x", "]"}]}], "=", "E"}], 
  TraditionalForm]]]], "TextNumbered",
 CellChangeTimes->{{3.6227606626491117`*^9, 3.622760694443968*^9}, {
  3.6227607628092885`*^9, 3.622760764712492*^9}, {3.622760824091196*^9, 
  3.622760825557599*^9}}],

Cell[TextData[{
 "We call the first term (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["1", "2"], "m", " ", 
    SuperscriptBox["v", "2"]}], TraditionalForm]]],
 ") the ",
 StyleBox["kinetic energy",
  FontSlant->"Italic",
  FontVariations->{"Underline"->True}],
 " and the second term (",
 Cell[BoxData[
  FormBox[
   RowBox[{"V", "[", "x", "]"}], TraditionalForm]]],
 ") the ",
 StyleBox["potential energy",
  FontSlant->"Italic",
  FontVariations->{"Underline"->True}],
 ". If we consider the particle at two different positions ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "1"], TraditionalForm]]],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "2"], TraditionalForm]]],
 ", the constant ",
 Cell[BoxData[
  FormBox["E", TraditionalForm]]],
 " must be the same in the above equation for both positions and hence we find"
}], "Text",
 CellChangeTimes->{{3.6227608373480215`*^9, 3.622760874119286*^9}, {
   3.622760929145072*^9, 3.6227609330212936`*^9}, {3.6227610066577497`*^9, 
   3.622761080076479*^9}, 3.6227789233645797`*^9}],

Cell[TextData[Cell[BoxData[
 FormBox[GridBox[{
    {GridBox[{
       {
        RowBox[{
         RowBox[{
          RowBox[{
           FractionBox["1", "2"], "m", " ", 
           SuperscriptBox[
            RowBox[{"v", "[", 
             SubscriptBox["x", "2"], "]"}], "2"]}], "-", 
          RowBox[{
           FractionBox["1", "2"], "m", " ", 
           SuperscriptBox[
            RowBox[{"v", "[", 
