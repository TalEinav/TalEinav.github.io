 FormBox[
  RowBox[{"0", "=", 
   RowBox[{"E", "=", 
    RowBox[{
     RowBox[{
      FractionBox["1", "2"], "m", " ", 
      SuperscriptBox["v", "2"]}], "-", 
     RowBox[{"A", " ", 
      SuperscriptBox["x", "n"]}]}]}]}], 
  TraditionalForm]],ExpressionUUID->"aea50d99-5d0c-44e4-b353-f63db4eab5a7"]], \
"TextNumbered",
