Cell[TextData[{
 StyleBox["Solution",
  FontVariations->{"Underline"->True}],
 "\nBy inspection we can tell that this ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["F", "\[RightVector]"], "[", 
    OverscriptBox["r", "\[RightVector]"], "]"}], TraditionalForm]],
  ExpressionUUID->"a926fad0-e7ec-4f83-a034-6b254ac8cc96"],
 " will generate a rotation in the ",
 Cell[BoxData[
  FormBox[
   RowBox[{"-", 
    OverscriptBox["z", "^"]}], TraditionalForm]],ExpressionUUID->
  "0f79faad-47fd-4b38-aa34-5c45d3083010"],
 " at all points in space (not just at the origin). Mathematically, "
}], "Text",
 CellChangeTimes->{{3.6227713971021285`*^9, 3.622771397663729*^9}, {
  3.622771441497815*^9, 3.6227714874344964`*^9}, {3.7168414446539145`*^9, 
  3.7168414453209753`*^9}},ExpressionUUID->"e43e4e36-a45d-48cc-a647-\
6f61a30c6f4a"],

Cell[TextData[Cell[BoxData[
 FormBox[GridBox[{
    {GridBox[{
       {
        RowBox[{
         RowBox[{
          OverscriptBox["\[Del]", "\[RightVector]"], 
          StyleBox["\[Cross]",
           FontWeight->"Plain"], 
          OverscriptBox["F", "\[RightVector]"]}], 
         StyleBox["=",
          FontWeight->"Plain"], 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             FractionBox[
              RowBox[{"\[PartialD]", 
               SubscriptBox["F", "z"]}], 
              RowBox[{"\[PartialD]", "y"}]], "-", 
             FractionBox[
              RowBox[{"\[PartialD]", 
               SubscriptBox["F", "y"]}], 
              RowBox[{"\[PartialD]", "z"}]]}], ")"}], 
           OverscriptBox["x", "^"]}], "+", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             FractionBox[
              RowBox[{"\[PartialD]", 
               SubscriptBox["F", "x"]}], 
              RowBox[{"\[PartialD]", "z"}]], "-", 
             FractionBox[
              RowBox[{"\[PartialD]", 
               SubscriptBox["F", "z"]}], 
              RowBox[{"\[PartialD]", "x"}]]}], ")"}], 
           OverscriptBox["y", "^"]}], "+", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             FractionBox[
              RowBox[{"\[PartialD]", 
               SubscriptBox["F", "y"]}], 
              RowBox[{"\[PartialD]", "x"}]], "-", 
             FractionBox[
              RowBox[{"\[PartialD]", 
               SubscriptBox["F", "x"]}], 
              RowBox[{"\[PartialD]", "y"}]]}], ")"}], 
           OverscriptBox["z", "^"]}]}]}]},
       {
        RowBox[{
         StyleBox["=",
          FontWeight->"Plain"], 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"-", "1"}], "-", "1"}], ")"}], 
          OverscriptBox["z", "^"]}]}]},
       {
        RowBox[{"=", 
         RowBox[{
          RowBox[{"-", "2"}], " ", 
          OverscriptBox["z", "^"]}]}]}
      },
