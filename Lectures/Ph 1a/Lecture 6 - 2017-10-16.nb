Cell[TextData[{
 "Recall that the ",
 StyleBox["Taylor series",
  FontSlant->"Italic",
  FontVariations->{"Underline"->True}],
 " of a function ",
 Cell[BoxData[
  FormBox[
   RowBox[{"f", "[", "x", "]"}], TraditionalForm]],ExpressionUUID->
  "eb6c2352-f349-46ec-977b-6eb2432f572d"],
