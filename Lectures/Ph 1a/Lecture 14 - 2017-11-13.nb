Cell[TextData[{
 "This week, we are starting a brand new topic: gravitational orbits! For \
this entire lecture, we will consider a mass ",
 Cell[BoxData[
  FormBox["m", TraditionalForm]],ExpressionUUID->
  "08e06893-6c4f-4211-a7ca-1a94a9e3d659"],
 " orbiting around a mass ",
 Cell[BoxData[
  FormBox["M", TraditionalForm]],ExpressionUUID->
  "30e9b876-6d43-408b-b516-094f76bd052e"],
 " ",
 StyleBox["fixed at the origin",
  FontSlant->"Italic"],
 "."
}], "Text",
 CellChangeTimes->{{3.7195077951154833`*^9, 3.719507880428836*^9}, {
  3.719507926258898*^9, 3.7195079597891803`*^9}},
 FontColor->GrayLevel[
  0],ExpressionUUID->"cb9eda17-685a-4329-873e-83d23e70230e"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"{", 
    RowBox[{"a", ",", "b"}], "}"}], "=", 
   RowBox[{"{", 
    RowBox[{"15", ",", "13"}], "}"}]}], ";", 
  RowBox[{"c", "=", 
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{
      SuperscriptBox["a", "2"], "-", 
      SuperscriptBox["b", "2"]}], ")"}], 
    RowBox[{"1", "/", "2"}]]}], ";", 
  RowBox[{"\[Epsilon]", "=", 
   FractionBox["c", "a"]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"x", "=", "12"}], ";", 
  RowBox[{"y", "=", 
   RowBox[{"b", 
    SqrtBox[
     RowBox[{"1", "-", 
      FractionBox[
       SuperscriptBox["x", "2"], 
       SuperscriptBox["a", "2"]]}]]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"x2", "=", "11.99"}], ";", 
  RowBox[{"y2", "=", 
   RowBox[{"b", 
    SqrtBox[
     RowBox[{"1", "-", 
      FractionBox[
       SuperscriptBox["x2", "2"], 
       SuperscriptBox["a", "2"]]}]]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"centerPlot", "@", 
  RowBox[{"Graphics", "[", 
