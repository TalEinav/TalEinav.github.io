  3.625109989411582*^9, 3.625109993055217*^9}, {3.6252043491565533`*^9, 
  3.625204349328154*^9}}],

Cell[TextData[Cell[BoxData[
 FormBox[GridBox[{
    {GridBox[{
       {
        RowBox[{"E", "=", "\[AlignmentMarker]", 
         RowBox[{
          RowBox[{
           FractionBox["1", "2"], "m", " ", 
           SuperscriptBox[
            OverscriptBox["r", "."], "2"]}], "+", 
