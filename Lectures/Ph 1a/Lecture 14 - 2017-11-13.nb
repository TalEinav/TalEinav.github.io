             RowBox[{
              OverscriptBox["r", "\[RightVector]"], "'"}]}]}]}]}]}]},
       {
        RowBox[{"=", 
         RowBox[{
          SubsuperscriptBox["\[Integral]", 
           OverscriptBox["r", "\[RightVector]"], 
           OverscriptBox["\[Infinity]", "\[RightVector]"]], 
          RowBox[{
           RowBox[{
            SubscriptBox[
             OverscriptBox["F", "\[RightVector]"], "grav"], "[", 
            RowBox[{
             OverscriptBox["r", "\[RightVector]"], "'"}], "]"}], 
           "\[CenterDot]", 
           RowBox[{"\[DifferentialD]", 
            RowBox[{
             OverscriptBox["r", "\[RightVector]"], "'"}]}]}]}]}]},
       {
        RowBox[{"=", 
         RowBox[{
          SubsuperscriptBox["\[Integral]", 
           OverscriptBox["r", "\[RightVector]"], 
           OverscriptBox["\[Infinity]", "\[RightVector]"]], 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"-", 
              FractionBox[
               RowBox[{"G", " ", "M", " ", "m"}], 
               SuperscriptBox[
                RowBox[{"(", 
                 RowBox[{"r", "'"}], ")"}], "2"]]}], 
             OverscriptBox["r", "^"]}], ")"}], "\[CenterDot]", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"\[DifferentialD]", 
              RowBox[{"r", "'"}]}], 
             OverscriptBox["r", "^"]}], ")"}]}]}]}]},
       {
        RowBox[{"=", 
         RowBox[{
          SubsuperscriptBox["\[Integral]", 
           OverscriptBox["r", "\[RightVector]"], 
           OverscriptBox["\[Infinity]", "\[RightVector]"]], 
          RowBox[{
           RowBox[{"-", 
            FractionBox[
             RowBox[{"G", " ", "M", " ", "m"}], 
             SuperscriptBox[
              RowBox[{"(", 
               RowBox[{"r", "'"}], ")"}], "2"]]}], 
           RowBox[{"\[DifferentialD]", 
            RowBox[{"r", "'"}]}]}]}]}]},
       {
        RowBox[{"=", 
         SubsuperscriptBox[
          RowBox[{"(", 
           FractionBox[
            RowBox[{"G", " ", "M", " ", "m"}], 
            RowBox[{"r", "'"}]], ")"}], 
          RowBox[{
           RowBox[{
            OverscriptBox["r", "\[RightVector]"], "'"}], "=", 
           OverscriptBox["r", "\[RightVector]"]}], 
          RowBox[{
           RowBox[{
            OverscriptBox["r", "\[RightVector]"], "'"}], "=", 
           OverscriptBox["\[Infinity]", "\[RightVector]"]}]]}]},
       {
        RowBox[{"=", 
         RowBox[{"-", 
          FractionBox[
           RowBox[{"G", " ", "M", " ", "m"}], "r"]}]}]}
