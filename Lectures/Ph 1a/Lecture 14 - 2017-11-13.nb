Cell[TextData[{
 "For example, if we rotate ",
 Cell[BoxData[
  FormBox["v", TraditionalForm]],ExpressionUUID->
  "84c5e61d-4358-4a85-8394-a8fe83561e44"],
 " around in a circle (but leave its magnitude unchanged), then ",
 Cell[BoxData[
  FormBox["E", TraditionalForm]],ExpressionUUID->
  "885c7682-5a43-4b42-9fec-9a9244145eab"],
 " will be constant while ",
 Cell[BoxData[
  FormBox["L", TraditionalForm]],ExpressionUUID->
  "651a07b2-d49b-4f44-8031-8cc3f0609d4d"],
 " will change; ",
 Cell[BoxData[
  FormBox["L", TraditionalForm]],ExpressionUUID->
  "26658955-a49a-44c8-a898-19029e5a49b7"],
 " will reach its maximum value when ",
 Cell[BoxData[
  FormBox[
   OverscriptBox["v", "\[RightVector]"], TraditionalForm]],ExpressionUUID->
  "414b7f47-c249-4a2f-bf10-108fd7996849"],
 " is perpendicular to ",
 Cell[BoxData[
  FormBox[
   OverscriptBox["r", "\[RightVector]"], TraditionalForm]],ExpressionUUID->
  "c639a35c-a59c-46bb-acf1-b21e26a32f82"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"L", "=", "0"}], TraditionalForm]],ExpressionUUID->
  "d44e8bb4-975d-43c5-8e1b-a77ff27f86a3"],
 " when ",
 Cell[BoxData[
  FormBox[
   OverscriptBox["v", "\[RightVector]"], TraditionalForm]],ExpressionUUID->
  "be90a3a7-1763-4ad3-bd5b-8ea0563e20a4"],
 " is parallel to ",
 Cell[BoxData[
  FormBox[
   OverscriptBox["r", "\[RightVector]"], TraditionalForm]],ExpressionUUID->
  "fbc693ba-9175-42f3-811b-c091c881bd69"],
 "."
}], "Text",
